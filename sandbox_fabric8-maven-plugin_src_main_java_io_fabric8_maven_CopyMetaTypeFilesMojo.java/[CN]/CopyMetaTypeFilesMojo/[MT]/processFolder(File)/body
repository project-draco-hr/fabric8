{
  if (input.isDirectory()) {
    File[] files=input.listFiles();
    if (files != null) {
      for (      File file : files) {
        if (file.isFile() && isJar(file)) {
          processJar(file);
        }
 else         if (file.isDirectory()) {
          processFolder(file);
        }
      }
    }
  }
}
