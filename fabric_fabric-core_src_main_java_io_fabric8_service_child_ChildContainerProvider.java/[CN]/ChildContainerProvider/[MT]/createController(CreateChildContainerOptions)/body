{
  ChildContainerController answer=null;
  try {
    ChildContainerControllerFactory factory=childContainerControllerFactory;
    if (factory != null) {
      answer=factory.createController(options);
    }
  }
 catch (  Exception e) {
    LOG.warn("Caught: " + e,e);
  }
  if (answer == null) {
    answer=createKarafContainerController();
  }
  return answer;
}
