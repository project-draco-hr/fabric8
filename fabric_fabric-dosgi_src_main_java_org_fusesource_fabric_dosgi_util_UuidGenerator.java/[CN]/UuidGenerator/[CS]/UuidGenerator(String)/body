{
synchronized (UNIQUE_STUB) {
    int hashValue=prefix.hashCode();
    if (hashValue < 0) {
      hashValue=-hashValue;
    }
    this.seed=hashValue + UNIQUE_STUB + (instanceCount++)+ ":";
    this.seed=generateSanitizedId(this.seed);
    this.length=this.seed.length() + ("" + Long.MAX_VALUE).length();
  }
}
