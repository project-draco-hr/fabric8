{
  String answer=address;
  int idx=answer.lastIndexOf(":");
  if (idx > 0) {
    answer=answer.substring(0,idx);
  }
  idx=answer.lastIndexOf(":");
  if (idx > 0) {
    answer=answer.substring(idx + 1);
  }
  idx=answer.lastIndexOf("/");
  if (idx > 0) {
    answer=answer.substring(idx + 1);
  }
  return answer;
}
