{
  super.tightUnmarshal(wireFormat,o,dataIn,bs);
  DataArrayResponse info=(DataArrayResponse)o;
  if (bs.readBoolean()) {
    short size=dataIn.readShort();
    org.fusesource.gateway.handlers.detecting.protocol.openwire.command.DataStructure value[]=new org.fusesource.gateway.handlers.detecting.protocol.openwire.command.DataStructure[size];
    for (int i=0; i < size; i++) {
      value[i]=(org.fusesource.gateway.handlers.detecting.protocol.openwire.command.DataStructure)tightUnmarsalNestedObject(wireFormat,dataIn,bs);
    }
    info.setData(value);
  }
 else {
    info.setData(null);
  }
}
