{
  if (bytePos == 0) {
    arrayLimit++;
    if (arrayLimit >= data.length) {
      byte d[]=new byte[data.length * 2];
      System.arraycopy(data,0,d,0,data.length);
      data=d;
    }
  }
  if (value) {
    data[arrayPos]|=0x01 << bytePos;
  }
  bytePos++;
  if (bytePos >= 8) {
    bytePos=0;
    arrayPos++;
  }
}
