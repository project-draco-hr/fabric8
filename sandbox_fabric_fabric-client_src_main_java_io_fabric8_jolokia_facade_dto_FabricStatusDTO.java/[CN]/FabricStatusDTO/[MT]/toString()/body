{
  StringBuffer sb=new StringBuffer();
  if (profileStatusMap != null) {
    for (    ProfileRequirementsDTO status : profileStatusMap.values()) {
      if (sb.length() > 0)       sb.append(", \n");
      sb.append(status);
    }
  }
 else {
    sb.append("No ProfileStatus objects found...");
  }
  return sb.toString();
}
