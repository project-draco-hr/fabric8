{
  List<String> children=new ArrayList<String>();
  if (tree != null) {
    Pattern p=Pattern.compile(node + "/[^/]*");
    for (    String c : tree.getTree().keySet()) {
      if (p.matcher(c).matches()) {
        children.add(c.substring(c.lastIndexOf('/') + 1));
      }
    }
  }
  return children;
}
