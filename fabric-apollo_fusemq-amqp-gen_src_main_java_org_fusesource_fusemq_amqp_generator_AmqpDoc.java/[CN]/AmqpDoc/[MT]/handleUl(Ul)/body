{
  String comment="";
  comment+="<ul>\n";
  for (  Li li : ul.getLi()) {
    comment+="<li>\n";
    for (    Object uType : li.getPOrUl()) {
      if (uType instanceof P) {
        comment+=handleP((P)uType);
      }
 else {
        comment+=handleUl((Ul)uType);
      }
    }
    comment+="<\n></li>\n";
  }
  comment+="<\n></ul>\n";
  return comment;
}
