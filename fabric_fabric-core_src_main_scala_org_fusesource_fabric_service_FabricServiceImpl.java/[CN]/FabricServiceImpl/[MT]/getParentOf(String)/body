{
  if (zooKeeper != null) {
    try {
      return zooKeeper.getStringData(ZkPath.CONTAINER_PARENT.getPath(name)).trim();
    }
 catch (    KeeperException.NoNodeException e) {
    }
catch (    Throwable e) {
      LOGGER.debug("Failed to find parent " + name + ". This exception will be ignored.",e);
    }
  }
  return "";
}
