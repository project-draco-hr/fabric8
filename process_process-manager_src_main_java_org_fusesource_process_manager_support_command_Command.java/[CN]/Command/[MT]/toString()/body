{
  final StringBuffer sb=new StringBuffer();
  sb.append("Command");
  sb.append("{command=").append(command);
  sb.append(", successfulExitCodes=").append(successfulExitCodes);
  sb.append(", directory=").append(directory);
  sb.append(", timeLimit=").append(timeLimit);
  sb.append('}');
  return sb.toString();
}
