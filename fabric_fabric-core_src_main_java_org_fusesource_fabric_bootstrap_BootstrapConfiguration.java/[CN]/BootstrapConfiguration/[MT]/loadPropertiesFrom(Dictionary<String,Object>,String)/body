{
  InputStream is=null;
  Properties properties=new Properties();
  try {
    is=new FileInputStream(from);
    properties.load(is);
    for (    String key : properties.stringPropertyNames()) {
      dictionary.put(key,properties.get(key));
    }
  }
 catch (  Exception e) {
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      Exception e) {
      }
    }
  }
}
