{
  outputZipFile.getParentFile().mkdirs();
  OutputStream os=new FileOutputStream(outputZipFile);
  ZipOutputStream zos=new ZipOutputStream(os);
  try {
    String path="";
    FileFilter filter=null;
    zipDirectory(log,sourceDir,zos,path,filter);
  }
  finally {
    try {
      zos.close();
    }
 catch (    Exception e) {
    }
  }
}
