{
  try {
    for (    String entry : registeredProxies.get(type)) {
      if (zookeeper.isConnected()) {
        zookeeper.deleteWithChildren(entry);
      }
      registeredProxies.remove(type);
    }
  }
 catch (  Exception e) {
    LOGGER.error("Failed to remove maven proxy from registry.",e);
  }
}
