{
  int size=0;
  if (zookeeper.isConnected()) {
    try {
      if (exists(zookeeper,ZkPath.CLOUD_NODES.getPath()) != null) {
        size=getChildren(zookeeper,ZkPath.CLOUD_NODES.getPath()).size();
      }
    }
 catch (    Exception ex) {
    }
  }
 else {
    size=(int)cache.size();
  }
  return size;
}
