{
  ZipFile zipFile=new ZipFile(patchZip);
  try {
    List<PatchData> patches=extractPatch(zipFile);
    if (patches.isEmpty()) {
      log(WARN,"No patch to apply");
    }
 else {
      for (      PatchData data : patches) {
        applyPatch(data,zipFile);
      }
    }
  }
  finally {
    close(zipFile);
  }
}
