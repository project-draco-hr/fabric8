{
  try {
    if (isComplexDataObject(type)) {
      return unmarshall(type,value,exchange);
    }
    if (value instanceof ComplexDataObject) {
      marshall(type,(ComplexDataObject)value,exchange);
    }
    return null;
  }
 catch (  IOException e) {
    throw new RuntimeCamelException(e);
  }
}
