{
  if (message == null) {
    if (id != null) {
      QueueBrowser tempBrowser=getBrowser();
      Enumeration iter=tempBrowser.getEnumeration();
      while (iter.hasMoreElements()) {
        Message item=(Message)iter.nextElement();
        if (id.equals(item.getJMSMessageID())) {
          message=item;
          break;
        }
      }
      tempBrowser.close();
    }
  }
  return message;
}
