{
  String realm="karaf";
  try {
    Configuration config=configAdmin.getConfiguration(pid);
    if (config != null) {
      Dictionary props=config.getProperties();
      realm=(String)props.get(realmProperty);
    }
  }
 catch (  Exception e) {
    LOGGER.warn("Error reading the realm " + realmProperty,e);
  }
  return realm;
}
