{
  if (key == null)   throw new NullPointerException("Key can not be null");
  EStructuralFeature eFeature=eClass().getEStructuralFeature(key);
  if (eFeature == null)   throw new IllegalArgumentException("Key '" + key + "' is not valid for this structure");
  Object returnValue=eGet(eFeature);
  eSet(eFeature,value);
  return returnValue;
}
