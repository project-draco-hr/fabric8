{
  if (Boolean.parseBoolean(System.getProperty("org.fusesource.fusemq.amqp.codec.NoArrayEncoding"))) {
    return false;
  }
  if (val.getListCount() == 1) {
    return false;
  }
  E lastElement=null;
  int currentElement=0;
  int max=val.getListCount();
  for (int i=0; i < max; i++) {
    E le=val.get(i);
    if (lastElement != null && currentElement != 0) {
      if (le == null || le.getClass() != lastElement.getClass()) {
        return false;
      }
    }
    if (lastElement == null && currentElement != 0) {
      return false;
    }
    currentElement++;
    lastElement=le;
  }
  return true;
}
