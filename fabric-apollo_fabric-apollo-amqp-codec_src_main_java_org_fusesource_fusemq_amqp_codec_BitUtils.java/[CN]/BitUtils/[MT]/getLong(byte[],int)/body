{
  return ((long)(source[offset + 0] & 0xff) << 56) | ((long)(source[offset + 1] & 0xff) << 48) | ((long)(source[offset + 2] & 0xff) << 40)| ((long)(source[offset + 3] & 0xff) << 32)| ((long)(source[offset + 4] & 0xff) << 24)| ((long)(source[offset + 5] & 0xff) << 16)| ((long)(source[offset + 6] & 0xff) << 8)| ((long)(source[offset + 7] & 0xff));
}
