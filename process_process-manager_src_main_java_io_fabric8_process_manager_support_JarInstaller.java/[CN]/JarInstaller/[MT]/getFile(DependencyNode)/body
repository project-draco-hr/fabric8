{
  if (node != null) {
    Dependency dependency=node.getDependency();
    if (dependency != null) {
      Artifact artifact=dependency.getArtifact();
      if (artifact != null) {
        File file=artifact.getFile();
        if (file == null) {
          return mavenResolver.resolveFile(artifact);
        }
        return file;
      }
    }
  }
  return null;
}
