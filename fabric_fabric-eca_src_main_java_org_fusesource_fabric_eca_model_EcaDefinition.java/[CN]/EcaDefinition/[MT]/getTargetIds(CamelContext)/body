{
  List<String> result=new ArrayList<String>();
  for (  EcaEventPattern ecaEventPattern : eventPatterns) {
    result.add(ecaEventPattern.getTargetId());
  }
  String[] ids=testPattern(context,getPattern());
  if (ids != null) {
    for (    String id : ids) {
      result.add(id);
    }
  }
  return result;
}
