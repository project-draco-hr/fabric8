{
  try {
    Agent parent=null;
    String parentId=getParentOf(name);
    if (!parentId.isEmpty()) {
      parent=getAgent(parentId);
    }
    return new AgentImpl(parent,name,this);
  }
 catch (  FabricException e) {
    throw e;
  }
catch (  Exception e) {
    throw new FabricException(e);
  }
}
