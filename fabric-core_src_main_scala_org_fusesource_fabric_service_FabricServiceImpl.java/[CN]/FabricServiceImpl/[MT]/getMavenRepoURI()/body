{
  URI uri=null;
  try {
    uri=new URI(DEFAULT_REPO_URI);
    if (zooKeeper.exists(ZkPath.CONFIGS_MAVEN_REPO.getPath()) != null) {
      String mavenRepo=zooKeeper.getStringData(ZkPath.CONFIGS_MAVEN_REPO.getPath());
      uri=new URI(mavenRepo);
    }
  }
 catch (  Exception e) {
  }
  return uri;
}
