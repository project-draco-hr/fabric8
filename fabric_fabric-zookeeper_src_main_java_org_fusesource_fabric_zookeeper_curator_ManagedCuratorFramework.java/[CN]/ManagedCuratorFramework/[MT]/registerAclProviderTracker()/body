{
  try {
    closeQuietly(aclProviderTracker);
    this.aclProviderTracker=new ServiceTracker<ACLProvider,ACLProvider>(bundleContext,ACLProvider.class,new ACLProviderTracker(bundleContext,aclProviders));
    this.aclProviderTracker.open();
  }
 catch (  Exception ex) {
    LOGGER.warn("Failed to register acl provider tracker.");
  }
}
