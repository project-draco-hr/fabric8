  if (map != null) {
    Map<String,ProcessRequirements> requirementsMap=loadProcessRequirements(map);
    System.out.println("Require containers: " + requirementsMap);
    for (    ProcessRequirements requirements : requirementsMap.values()) {
    }
  }
}
