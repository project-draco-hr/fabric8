{
  return new DelegateZooKeeperGroup<CamelNodeState>(path,CamelNodeState.class){
    @Override public void start(){
      useCurator(curator);
      groups.add(this);
      super.start();
    }
    @Override public void close() throws IOException {
      groups.remove(this);
      super.close();
    }
  }
;
}
