{
  super.init(config);
  try {
    this.counter=new SharedCount(curator,path,0);
    this.counter.start();
  }
 catch (  Exception e) {
    LOGGER.error("Error starting SharedCount for ZkNode " + path + " due "+ e.getMessage(),e);
    throw new ServletException("Error starting SharedCount for ZkNode " + path,e);
  }
}
