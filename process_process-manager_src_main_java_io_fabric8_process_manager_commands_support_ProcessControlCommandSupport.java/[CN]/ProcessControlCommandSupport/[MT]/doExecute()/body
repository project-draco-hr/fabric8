{
  checkRequirements();
  Map<Integer,Installation> map=getProcessManager().listInstallationMap();
  for (  int id : ids) {
    Installation installation=map.get(id);
    if (installation == null) {
      System.out.println("No such process number: " + id);
    }
 else {
      doControlCommand(installation);
    }
  }
  return null;
}
