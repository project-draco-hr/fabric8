{
  if (zkMasterCache != null) {
    zkMasterCache=null;
  }
  DataStore dataStore=fabricService.get().adapt(DataStore.class);
  dataStore.untrackConfiguration(configurationChangeHandler);
  if (keepAliveTimer != null) {
    keepAliveTimer.cancel();
    keepAliveTimer=null;
  }
  deactivateComponent();
}
