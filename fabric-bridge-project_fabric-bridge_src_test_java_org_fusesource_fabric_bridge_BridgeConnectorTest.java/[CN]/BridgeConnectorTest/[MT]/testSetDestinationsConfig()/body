{
  connector.afterPropertiesSet();
  connector.setDestinationsConfig(connector.getInboundDestinations());
  connector.start();
  connector.setDestinationsConfig(connector.getOutboundDestinations());
  connector.stop();
  connector.setDestinationsConfig(connector.getInboundDestinations());
  connector.destroy();
  connector.setDestinationsConfig(connector.getInboundDestinations());
}
