{
  if (!contains(m_pid + MavenConstants.PROPERTY_CERTIFICATE_CHECK)) {
    return set(m_pid + MavenConstants.PROPERTY_CERTIFICATE_CHECK,Boolean.valueOf(m_propertyResolver.get(m_pid + MavenConstants.PROPERTY_CERTIFICATE_CHECK)));
  }
  return get(m_pid + MavenConstants.PROPERTY_CERTIFICATE_CHECK);
}
