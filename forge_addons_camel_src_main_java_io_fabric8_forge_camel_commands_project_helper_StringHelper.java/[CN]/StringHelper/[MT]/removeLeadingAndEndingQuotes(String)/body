{
  if (s == null || s.isEmpty()) {
    return s;
  }
  String copy=s.trim();
  if (copy.startsWith("'") && copy.endsWith("'")) {
    return copy.substring(1,copy.length() - 1);
  }
  if (copy.startsWith("\"") && copy.endsWith("\"")) {
    return copy.substring(1,copy.length() - 1);
  }
  return s;
}
