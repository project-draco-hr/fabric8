{
  if (other == null) {
    return false;
  }
  if (!header.equals(other.header)) {
    return false;
  }
  if (!extHeader.equals(other.extHeader)) {
    return false;
  }
  if (this.body != null && other.body == null) {
    return false;
  }
  if (body == null && other.body == null) {
    return true;
  }
  return body.toString().equals(other.body.toString());
}
