{
  Map<Object,Object> attributeMap=context.getUIContext().getAttributeMap();
  if (last) {
    String kind=mandatoryAttributeValue(attributeMap,"kind");
    if ("xml".equals(kind)) {
      return executeXml(context,attributeMap);
    }
 else {
      return executeJava(context,attributeMap);
    }
  }
 else {
    return null;
  }
}
