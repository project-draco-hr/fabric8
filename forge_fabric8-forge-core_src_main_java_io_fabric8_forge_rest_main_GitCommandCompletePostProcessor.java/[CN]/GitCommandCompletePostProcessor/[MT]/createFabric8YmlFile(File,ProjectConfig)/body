{
  if (!config.isEmpty()) {
    try {
      if (ProjectConfigs.saveToFolder(basedir,config,false)) {
        LOG.info("Generated " + ProjectConfigs.FILE_NAME + " file");
      }
    }
 catch (    IOException e) {
      LOG.warn("Failed to generate " + ProjectConfigs.FILE_NAME + " due to: "+ e,e);
    }
  }
}
