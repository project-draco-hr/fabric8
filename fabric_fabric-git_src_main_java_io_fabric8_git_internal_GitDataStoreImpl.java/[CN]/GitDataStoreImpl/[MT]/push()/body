{
  assertReadLock();
  try {
    GitOperation<Object> gitop=new GitOperation<Object>(){
      public Object call(      Git git,      GitContext context) throws Exception {
        context.requirePush();
        return null;
      }
    }
;
    executeRead(gitop);
  }
 catch (  Exception e) {
    LOGGER.warn("Failed to perform a pull " + e,e);
  }
}
