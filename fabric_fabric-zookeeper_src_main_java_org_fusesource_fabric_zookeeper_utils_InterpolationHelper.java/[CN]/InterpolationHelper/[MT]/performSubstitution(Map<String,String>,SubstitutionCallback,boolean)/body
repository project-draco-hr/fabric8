{
  for (  String name : properties.keySet()) {
    String value=properties.get(name);
    String substValue=substVars(value,name,null,properties,callback);
    if (substValue != null) {
      properties.put(name,substValue);
    }
 else     if (!keepPlaceHolderOnNull) {
      properties.put(name,"");
    }
  }
}
