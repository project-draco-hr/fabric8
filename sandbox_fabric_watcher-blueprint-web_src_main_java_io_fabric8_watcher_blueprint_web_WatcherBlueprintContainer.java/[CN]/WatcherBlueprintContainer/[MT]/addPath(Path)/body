{
  URL url=toUrl(path);
  if (url != null) {
    BlueprintContainer container=containerMap.get(url);
    if (container != null) {
      closeContainer(url,container);
    }
    try {
      container=createContainer(path,url);
      if (container != null) {
        containerMap.put(url,container);
      }
    }
 catch (    Exception e) {
      LOG.info("Failed to create container at " + url + ". "+ e,e);
    }
  }
}
