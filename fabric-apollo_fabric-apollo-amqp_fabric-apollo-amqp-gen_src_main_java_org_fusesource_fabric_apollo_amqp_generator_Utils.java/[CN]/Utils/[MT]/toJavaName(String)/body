{
  StringTokenizer tok=new StringTokenizer(name.trim(),"- ");
  String javaName="";
  int i=0;
  while (tok.hasMoreElements()) {
    String token=tok.nextToken();
    if (i > 0) {
      javaName+=token.substring(0,1).toUpperCase();
      javaName+=token.substring(1);
    }
 else {
      javaName+=token;
    }
    i++;
  }
  return javaName;
}
