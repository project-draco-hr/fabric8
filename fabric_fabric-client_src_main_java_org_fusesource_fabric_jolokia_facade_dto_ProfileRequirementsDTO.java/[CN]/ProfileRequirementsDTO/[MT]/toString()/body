{
  String deps="";
  if (dependentProfiles != null) {
    for (    String dep : dependentProfiles) {
      if (deps.length() > 0)       deps+=", ";
      deps+=dep;
    }
  }
  return String.format("ProfileRequirements: (profile: %s, minimumInstances: %d, maximumInstances: %d, dependentProfiles: %s)",profile,minimumInstances,maximumInstances,deps);
}
