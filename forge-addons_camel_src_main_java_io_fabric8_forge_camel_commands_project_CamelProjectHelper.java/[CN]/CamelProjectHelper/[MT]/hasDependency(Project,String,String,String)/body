{
  List<Dependency> dependencies=project.getFacet(DependencyFacet.class).getEffectiveDependencies();
  for (  Dependency d : dependencies) {
    if (d.getCoordinate().getGroupId().equals(groupId) && d.getCoordinate().getArtifactId().equals(artifactId) && d.getCoordinate().getVersion().equals(version)) {
      return true;
    }
  }
  return false;
}
