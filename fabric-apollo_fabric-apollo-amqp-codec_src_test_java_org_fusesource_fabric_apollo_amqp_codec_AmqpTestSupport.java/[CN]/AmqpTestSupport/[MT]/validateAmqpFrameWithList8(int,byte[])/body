{
  int oldOffset=offset;
  long frameSize=BitUtils.getUInt(b,offset);
  offset+=8;
  offset+=1;
  offset+=1;
  int typeLength=0xff & BitUtils.getUByte(b,offset);
  offset+=1 + typeLength;
  offset+=1;
  typeLength=0xff & BitUtils.getUByte(b,offset);
  offset+=1;
  offset+=typeLength;
  assertEquals(frameSize,offset - oldOffset);
  return offset;
}
