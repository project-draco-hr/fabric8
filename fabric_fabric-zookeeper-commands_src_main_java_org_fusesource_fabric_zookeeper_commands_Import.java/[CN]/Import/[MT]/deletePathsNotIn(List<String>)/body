{
  List<String> zkPaths=getZooKeeper().getAllChildren(target);
  for (  String path : zkPaths) {
    path="/" + path;
    if (!paths.contains(path)) {
      if (!dryRun) {
        getZooKeeper().deleteWithChildren(path);
      }
 else {
        System.out.printf("Deleting path %s and everything under it\n",path);
      }
    }
  }
}
