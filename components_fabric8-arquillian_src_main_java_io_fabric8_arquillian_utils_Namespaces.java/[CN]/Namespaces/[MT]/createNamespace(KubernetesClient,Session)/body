{
  return client.namespaces().createNew().withNewMetadata().withName(session.getNamespace()).addToLabels("project",client.getNamespace()).addToLabels("provider","fabric8").addToLabels("component","integrationTest").addToLabels("framework","arquillian").withAnnotations(createNamespaceAnnotations(session,Constants.RUNNING_STATUS)).endMetadata().done();
}
