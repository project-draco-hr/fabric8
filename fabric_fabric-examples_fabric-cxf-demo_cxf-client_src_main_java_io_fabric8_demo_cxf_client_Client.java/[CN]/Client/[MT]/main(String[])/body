{
  Client client=new Client();
  System.out.println("Calling the web service 10 times with 5 second delay ...");
  for (int i=0; i < 10; i++) {
    System.out.println("Calling the sayHello #" + i + " with result: "+ client.getProxy().sayHello());
    Thread.sleep(5000);
  }
  System.out.println("... Done");
}
