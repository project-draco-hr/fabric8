{
  if (mbeanServer != null) {
    try {
      ObjectName name=getMbeanName();
      if (name != null && mbeanServer.isRegistered(name)) {
        mbeanServer.unregisterMBean(name);
      }
    }
 catch (    Exception e) {
      LOG.warn("An error occurred during mbean server registration: " + e,e);
    }
  }
}
