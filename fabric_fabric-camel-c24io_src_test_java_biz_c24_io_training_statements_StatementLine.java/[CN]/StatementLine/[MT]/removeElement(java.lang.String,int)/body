{
  name=unmakeSubstitution(name,index);
switch (Arrays.binarySearch(NATIVE_ELEMENTS,name)) {
case 1:
    setPostingDate(null);
  return;
case 4:
setValueDate(null);
return;
case 0:
setDrCr(null);
return;
case 3:
