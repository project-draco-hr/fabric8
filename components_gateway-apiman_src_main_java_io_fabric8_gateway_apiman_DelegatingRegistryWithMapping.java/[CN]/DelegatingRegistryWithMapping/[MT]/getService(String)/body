{
  path=getServiceBindPath(path);
  if (path.contains("?"))   path=path.substring(0,path.indexOf("?") - 1);
  if (path.contains("#"))   path=path.substring(0,path.indexOf("#") - 1);
  if (serviceBindPaths.containsKey(path)) {
    return serviceBindPaths.get(path);
  }
 else {
    return null;
  }
}
