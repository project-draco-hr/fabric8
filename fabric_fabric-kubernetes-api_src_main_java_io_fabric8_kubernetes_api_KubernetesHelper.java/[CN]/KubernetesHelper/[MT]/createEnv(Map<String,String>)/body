{
  List<Env> answer=new ArrayList<>();
  Set<Map.Entry<String,String>> entries=environmentVariables.entrySet();
  for (  Map.Entry<String,String> entry : entries) {
    Env env=new Env();
    env.setName(entry.getKey());
    env.setValue(entry.getValue());
    answer.add(env);
  }
  return answer;
}
