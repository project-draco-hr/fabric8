{
  preCreateContainer(name);
  CreateContainerOptions args=CreateContainerOptionsBuilder.jclouds().name(name).ensembleServer(isEnsembleServer).credential(credential).debugContainer(debugContainer).group(group).hardwareId(hardwareId).identity(identity).imageId(imageId).instanceType(instanceType).locationId(locationId).number(number).owner(owner).providerName(providerName).user(user).proxyUri(proxyUri != null ? proxyUri : fabricService.getMavenRepoURI()).zookeeperUrl(fabricService.getZookeeperUrl());
  ;
  Container[] containers=fabricService.createContainers(args);
  postCreateContainer(containers);
  return null;
}
