{
  StringBuffer buffer=new StringBuffer();
  for (  T element : logEvents) {
    if (predicate.matches(element)) {
      return;
    }
    if (buffer.length() > 0) {
      buffer.append(", ");
    }
    buffer.append(element);
  }
  fail("Could not find message matching " + predicate + " when found: "+ buffer);
}
