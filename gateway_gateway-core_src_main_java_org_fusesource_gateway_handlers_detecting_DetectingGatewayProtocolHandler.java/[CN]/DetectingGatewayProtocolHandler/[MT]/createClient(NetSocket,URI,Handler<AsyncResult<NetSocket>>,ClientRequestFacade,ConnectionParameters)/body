{
  NetClient client=vertx.createNetClient();
  int port=url.getPort();
  String host=url.getHost();
  LOG.info(String.format("Connecting '%s' requesting host '%s' with client key '%s' to '%s:%d' using the %s protocol",socket.remoteAddress(),params.protocolVirtualHost,facade.getClientRequestKey(),host,port,params.protocol));
  return client.connect(port,host,handler);
}
