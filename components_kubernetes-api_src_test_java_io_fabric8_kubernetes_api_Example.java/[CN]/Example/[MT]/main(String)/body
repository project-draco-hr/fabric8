{
  KubernetesClient kube=new DefaultKubernetesClient();
  try {
    listPods(kube);
    listReplicationControllers(kube);
    listServices(kube);
    listServiceAccounts(kube);
    listEndpoints(kube);
  }
 catch (  Exception e) {
    System.out.println("FAILED: " + e);
    e.printStackTrace();
  }
 finally {
    kube.close();
  }
}
