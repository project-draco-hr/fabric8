{
  if (value.getListCount() == 0) {
    out.writeByte(Encoder.NULL_FORMAT_CODE);
    return;
  }
  boolean first=true;
  for (  E le : value) {
    Encoded<?> encoded=le.getBuffer(MARSHALLER).getEncoded();
    if (first) {
      encoded.marshal(out);
      first=false;
    }
 else {
      encoded.marshalConstructor(out);
      encoded.marshalData(out);
    }
  }
}
