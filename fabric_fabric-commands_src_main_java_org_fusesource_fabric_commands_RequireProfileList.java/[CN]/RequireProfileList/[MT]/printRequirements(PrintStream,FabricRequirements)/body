{
  out.println(String.format("%-40s %-14s %-14s %s","[profile]","[# minimum]","[# maximum]","[depends on]"));
  List<ProfileRequirements> profileRequirements=requirements.getProfileRequirements();
  for (  ProfileRequirements profile : profileRequirements) {
    out.println(String.format("%-40s %-14s %-14s %s",profile.getProfile(),getStringOrBlank(profile.getMinimumInstances()),getStringOrBlank(profile.getMaximumInstances()),getStringOrBlank(profile.getDependentProfiles())));
  }
}
