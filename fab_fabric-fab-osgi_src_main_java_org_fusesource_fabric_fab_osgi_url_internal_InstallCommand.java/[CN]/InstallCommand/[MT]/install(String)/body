{
  try {
    return getBundleContext().installBundle(location,null);
  }
 catch (  BundleException ex) {
    if (ex.getNestedException() != null) {
      println(ex.getNestedException().toString());
    }
 else {
      println(ex.toString());
    }
  }
catch (  Exception ex) {
    println(ex.toString());
  }
  return null;
}
