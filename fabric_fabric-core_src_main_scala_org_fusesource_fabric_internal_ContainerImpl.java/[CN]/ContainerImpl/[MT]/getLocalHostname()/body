{
  try {
    if (service.getZooKeeper().exists(ZkPath.CONTAINER_LOCAL_HOSTNAME.getPath(id)) == null) {
      return null;
    }
 else {
      return service.getZooKeeper().getStringData(ZkPath.CONTAINER_LOCAL_HOSTNAME.getPath(id));
    }
  }
 catch (  Exception e) {
    throw new FabricException(e);
  }
}
