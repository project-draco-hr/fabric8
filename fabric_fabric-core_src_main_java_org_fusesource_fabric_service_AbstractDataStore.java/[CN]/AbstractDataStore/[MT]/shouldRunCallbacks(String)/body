{
  String karafName=runtimeProperties.get().getProperty(SystemProperties.KARAF_NAME);
  String currentVersion=getContainerVersion(karafName);
  return path.equals(ZkPath.CONFIG_ENSEMBLES.getPath()) || path.equals(ZkPath.CONFIG_ENSEMBLE_URL.getPath()) || path.equals(ZkPath.CONFIG_ENSEMBLE_PASSWORD.getPath())|| path.equals(ZkPath.CONFIG_CONTAINER.getPath(karafName))|| (currentVersion != null && path.equals(ZkPath.CONFIG_VERSIONS_CONTAINER.getPath(currentVersion,karafName)));
}
