{
  String header="com.acme.dictionary; from:String=nl; to=de; version:Version=3.4; indices:List<Long>=\" 23 , 45 \", " + "com.acme.dictionary; from:String=de; to=nl; version:Version=4.1, " + "com.acme.ip2location;country:List<String>=\"nl,be,fr,uk\";version:Version=1.3, "+ "com.acme.seps;        tokens:List<String>=\";,\\\",\\,\"";
  List<Capability> caps=ResourceBuilder.parseCapability(null,header);
  System.out.println("Header: " + header);
  for (  Capability bc : caps) {
    System.out.println(bc);
    List<Capability> cap=ResourceBuilder.parseCapability(null,bc.toString());
    assertEquals(1,cap.size());
    assertEquals(cap.get(0).toString(),bc.toString());
    assertEquals(cap.get(0),bc);
  }
}
