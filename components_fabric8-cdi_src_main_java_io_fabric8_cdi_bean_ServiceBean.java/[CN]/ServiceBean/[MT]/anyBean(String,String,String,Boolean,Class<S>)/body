{
  for (  Map.Entry<Key,ServiceBean> entry : BEANS.entrySet()) {
    Key key=entry.getKey();
    if (key.serviceName.equals(id) && key.serviceProtocol.equals(protocol) && key.servicePort.equals(port)&& key.type.equals(type)) {
      return entry.getValue();
    }
  }
  return getBean(id,protocol,port,null,external,type);
}
