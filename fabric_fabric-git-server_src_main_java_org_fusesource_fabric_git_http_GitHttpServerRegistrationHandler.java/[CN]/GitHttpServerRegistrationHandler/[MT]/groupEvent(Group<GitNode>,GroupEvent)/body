{
  if (isValid()) {
    if (group.isMaster()) {
      LOGGER.info("Git repo is the master");
    }
 else {
      LOGGER.info("Git repo is not the master");
    }
    try {
      GitNode state=createState();
      group.update(state);
      String url=state.getUrl();
      gitRemoteUrl=getSubstitutedData(curator.get(),url);
      if (group.isMaster()) {
        updateConfigAdmin();
      }
    }
 catch (    Exception e) {
    }
  }
}
