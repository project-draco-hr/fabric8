{
  try {
    if (httpService != null) {
      if (connected) {
        try {
          group.close();
        }
 catch (        IOException e) {
        }
      }
      httpService.unregister("/git");
    }
  }
 catch (  Exception ex) {
    LOGGER.warn("Http service returned error on servlet unregister. Possibly the service has already been stopped");
  }
  this.httpService=null;
}
