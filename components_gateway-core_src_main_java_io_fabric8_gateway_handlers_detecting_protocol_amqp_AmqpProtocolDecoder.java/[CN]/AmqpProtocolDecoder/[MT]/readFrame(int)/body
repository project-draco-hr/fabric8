{
  return new Action<AmqpEvent>(){
    public AmqpEvent apply() throws IOException {
      Buffer frameData=readBytes(size);
      if (frameData != null) {
        nextDecodeAction=readFrameSize;
        return new AmqpEvent(AmqpEvent.Type.FRAME,frameData,null);
      }
 else {
        return null;
      }
    }
  }
;
}
