{
  try {
    byte[] buffer=new byte[8192];
    int len;
    for (; ; ) {
      len=inputStream.read(buffer);
      if (len > 0) {
        outputStream.write(buffer,0,len);
      }
 else {
        outputStream.flush();
        break;
      }
    }
  }
  finally {
    try {
      inputStream.close();
    }
 catch (    IOException e) {
    }
    try {
      outputStream.close();
    }
 catch (    IOException e) {
    }
  }
}
