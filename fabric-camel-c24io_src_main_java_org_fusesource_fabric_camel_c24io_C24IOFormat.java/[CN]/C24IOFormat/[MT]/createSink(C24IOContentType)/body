{
switch (content) {
case Default:
    return getDefaultSink();
case Binary:
  return new BinarySink();
case Java:
return new JavaClassSink();
case Sax:
return new SAXSink();
case Text:
return new TextualSink();
case Xml:
return new XMLSink();
case TagValuePair:
return new TagValuePairSink();
default :
throw new IllegalArgumentException("Unknown format type: " + content);
}
}
