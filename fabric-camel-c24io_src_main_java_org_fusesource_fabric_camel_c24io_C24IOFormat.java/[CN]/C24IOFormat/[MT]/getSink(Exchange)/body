{
  if (sink == null) {
    C24IOContentType content=getContentType();
    if (content != null) {
      if (content == C24IOContentType.Auto) {
        return discoverSink(exchange);
      }
      sink=createSink(content);
    }
    if (sink == null) {
      sink=getDefaultSink();
    }
  }
  return sink;
}
