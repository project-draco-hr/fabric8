{
  try {
    mbeanServer.removeNotificationListener(new ObjectName("JMImplementation:type=MBeanServerDelegate"),this);
    unregisterDomains();
  }
 catch (  Exception e) {
    logger.warn("An error occured during mbean server unregistration",e);
  }
  mbeanServer=null;
  bundleContext.ungetService(ref);
}
