{
  String userInfos=users.get(username);
  if (userInfos != null) {
    for (    RolePrincipal rp : listRoles(username)) {
      if (role.equals(rp.getName())) {
        return;
      }
    }
    String newUserInfos=userInfos + "," + role;
    users.put(username,newUserInfos);
  }
  saveUserProperties();
}
