{
  BundleContext context=createMock(BundleContext.class);
  expect(context.getServiceReferences("org.apache.aries.blueprint.NamespaceHandler","(key=value)")).andReturn(null);
  replay(context);
  assertFalse("Service is not available",Services.isAvailable(context,"org.apache.aries.blueprint.NamespaceHandler",Services.createProperties("key","value")));
  verify(context);
}
