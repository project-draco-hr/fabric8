{
  try {
    if (zooKeeper.exists(ZkPath.CONFIG_VERSION.getPath(name)) == null) {
      throw new FabricException("Version '" + name + "' does not exist!");
    }
    return new VersionImpl(name,this);
  }
 catch (  FabricException e) {
    throw e;
  }
catch (  Exception e) {
    throw new FabricException(e);
  }
}
