{
  return new Action<AmqpEvent>(){
    public AmqpEvent apply() throws IOException {
      Buffer magic=readBytes(8);
      if (magic != null) {
        nextDecodeAction=readFrameSize;
        return new AmqpEvent(AmqpEvent.Type.HEADER,magic,new AmqpHeader(magic));
      }
 else {
        return null;
      }
    }
  }
;
}
