{
  if (project != null) {
    MavenFacet maven=project.getFacet(MavenFacet.class);
    if (maven != null) {
      Model pom=maven.getModel();
      if (pom != null) {
        Properties properties=pom.getProperties();
        return properties.getProperty("docker.from");
      }
    }
  }
  return null;
}
