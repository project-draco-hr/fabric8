{
  if (startCounter.decrementAndGet() == 0) {
    running.set(false);
    try {
      if (group != null) {
        group.close();
      }
    }
 catch (    Throwable ignore) {
    }
    if (managedZkClient) {
      try {
        curator.close();
      }
 catch (      Throwable ignore) {
      }
      curator=null;
    }
  }
}
