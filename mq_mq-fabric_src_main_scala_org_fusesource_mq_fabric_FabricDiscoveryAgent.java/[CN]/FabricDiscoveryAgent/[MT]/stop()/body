{
  if (startCounter.decrementAndGet() == 0) {
    running.set(false);
    try {
      group.close();
    }
 catch (    Throwable ignore) {
    }
    if (managedZkClient) {
      try {
        curator.close();
      }
 catch (      Throwable ignore) {
      }
      curator=null;
    }
  }
  if (tracker != null) {
    LOG.info("closing tracker");
    tracker.close();
  }
}
