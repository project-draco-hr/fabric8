{
  try {
    while (true) {
      TreeData data=currentData.get(fullPath);
      if (data.isInvalidated()) {
        currentData.invalidate(fullPath);
      }
 else {
        return data;
      }
    }
  }
 catch (  ExecutionException e) {
    return null;
  }
catch (  CacheLoader.InvalidCacheLoadException e) {
    return null;
  }
}
