{
  String str=name != null ? "eca(" + name + ")" : "route";
  str+="[" + getInputs() + "-> ";
  if (ecaDefinition != null && ecaDefinition.getEventPatterns() != null && ecaDefinition.getEventPatterns().isEmpty() == false) {
    str+="[" + ecaDefinition.getEventPatterns() + "]";
  }
  str+=" ->" + getOutputs() + "]";
  return str;
}
