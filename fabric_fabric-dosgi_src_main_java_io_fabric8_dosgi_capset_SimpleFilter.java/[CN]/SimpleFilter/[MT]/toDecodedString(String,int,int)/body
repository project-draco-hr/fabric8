{
  StringBuffer sb=new StringBuffer(endIdx - startIdx);
  boolean escaped=false;
  for (int i=0; i < (endIdx - startIdx); i++) {
    char c=s.charAt(startIdx + i);
    if (!escaped && (c == '\\')) {
      escaped=true;
    }
 else {
      escaped=false;
      sb.append(c);
    }
  }
  return sb.toString();
}
