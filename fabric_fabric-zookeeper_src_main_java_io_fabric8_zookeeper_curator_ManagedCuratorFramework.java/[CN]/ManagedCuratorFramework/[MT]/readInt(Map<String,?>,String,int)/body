{
  try {
    Object obj=props.get(key);
    if (obj instanceof Number) {
      return ((Number)obj).intValue();
    }
 else     if (obj instanceof String) {
      return Integer.parseInt((String)obj);
    }
 else {
      return defaultValue;
    }
  }
 catch (  Exception e) {
    return defaultValue;
  }
}
