{
  return new RouteBuilder(){
    public void configure(){
      Namespaces ns=new Namespaces("foo","urn:iso:std:iso:20022:tech:xsd:pacs.008.001.01");
      from("file:src/test/data?noop=true").unmarshal().c24io(DocumentElement.class,C24IOContentType.Xml).filter(ns.xquery("//foo:MsgId = 'PFSM1234'")).to("mock:result");
    }
  }
;
}
