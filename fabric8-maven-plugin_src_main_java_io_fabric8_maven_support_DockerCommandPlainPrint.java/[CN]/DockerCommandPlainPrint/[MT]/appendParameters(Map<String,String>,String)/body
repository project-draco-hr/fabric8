{
  for (  Map.Entry<String,String> entry : data.entrySet()) {
    dockerPlainTextCommand.append(flag);
    dockerPlainTextCommand.append(" ");
    dockerPlainTextCommand.append(entry.getKey());
    dockerPlainTextCommand.append("=");
    dockerPlainTextCommand.append(entry.getValue());
    dockerPlainTextCommand.append(" ");
  }
}
