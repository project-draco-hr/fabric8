{
  PomDetails details=resolvePomDetails();
  Objects.notNull(details,"pomDetails");
  try {
    return getResolver().collectDependencies(details,offline);
  }
 catch (  IOException e) {
    logFailure(e);
    throw e;
  }
catch (  XmlPullParserException e) {
    logFailure(e);
    throw e;
  }
catch (  RepositoryException e) {
    logFailure(e);
    throw e;
  }
}
