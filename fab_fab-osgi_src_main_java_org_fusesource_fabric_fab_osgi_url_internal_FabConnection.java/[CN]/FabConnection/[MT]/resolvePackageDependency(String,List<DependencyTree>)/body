{
  for (  DependencyTree dependency : dependencies) {
    try {
      Set<String> packages=dependency.getPackages();
      if (packages.contains(packageName)) {
        return dependency;
      }
    }
 catch (    IOException e) {
      LOG.warn("Failed to get the packages on dependency: " + dependency + ". "+ e,e);
    }
  }
  return null;
}
