{
  if (rootTree == null) {
    PomDetails details=resolvePomDetails();
    Objects.notNull(details,"pomDetails");
    try {
      rootTree=getResolver().collectDependencies(details,offline).getTree();
    }
 catch (    IOException e) {
      logFailure(e);
      throw e;
    }
catch (    XmlPullParserException e) {
      logFailure(e);
      throw e;
    }
catch (    RepositoryException e) {
      logFailure(e);
      throw e;
    }
  }
  return rootTree;
}
