{
  FabricService fabric=getFabricService();
  String version=requirements.getVersion();
  if (Strings.isEmpty(version)) {
    Version defaultVersion=fabric.getDefaultVersion();
    if (defaultVersion != null) {
      version=defaultVersion.getId();
    }
  }
  if (Strings.isEmpty(version)) {
    version="1.0";
  }
  return version;
}
