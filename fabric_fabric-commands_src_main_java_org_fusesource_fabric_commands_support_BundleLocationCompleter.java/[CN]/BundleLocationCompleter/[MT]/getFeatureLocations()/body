{
  Set<String> bundleLocations=new LinkedHashSet<String>();
  for (  Feature feature : featuresService.listFeatures()) {
    try {
      for (      BundleInfo info : feature.getBundles()) {
        bundleLocations.add(info.getLocation());
      }
    }
 catch (    Exception e) {
    }
  }
  return bundleLocations;
}
