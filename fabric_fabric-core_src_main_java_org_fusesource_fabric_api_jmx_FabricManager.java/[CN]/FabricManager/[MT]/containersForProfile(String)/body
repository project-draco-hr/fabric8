{
  List<Container> answer=new ArrayList<Container>();
  if (profileId != null) {
    for (    Container c : getFabricService().getContainers()) {
      for (      Profile p : c.getProfiles()) {
        if (profileId.equals(p.getId())) {
          answer.add(c);
        }
      }
    }
  }
  return answer;
}
