{
  buf.setLength(0);
  while (c != '"') {
    if (c == '\\') {
      next();
      if (c == 'u') {
        add(unicode());
      }
 else {
        Object value=escapes.get(Character.valueOf(c));
        if (value != null) {
          add(((Character)value).charValue());
        }
      }
    }
 else {
      add();
    }
  }
  next();
  return buf.toString();
}
