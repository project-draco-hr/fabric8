{
  String actual=name.replace('.','/') + ".class";
  byte[] bytes=null;
  if (bytes != null) {
synchronized (this) {
      Class clazz=findLoadedClass(name);
      if (clazz == null) {
        clazz=defineClass(name,bytes,0,bytes.length);
      }
      return clazz;
    }
  }
  throw new ClassNotFoundException(name);
}
