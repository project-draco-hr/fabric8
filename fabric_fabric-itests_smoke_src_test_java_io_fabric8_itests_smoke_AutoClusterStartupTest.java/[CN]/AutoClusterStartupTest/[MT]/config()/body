{
  return new Option[]{new DefaultCompositeOption(fabricDistributionConfiguration()),KarafDistributionOption.editConfigurationFilePut("etc/users.properties",USERNAME,String.format("%s,%s",PASSOWRD,ROLE)),new VMOption("-D" + CreateEnsembleOptions.ENSEMBLE_AUTOSTART + "=true"),new VMOption("-D" + CreateEnsembleOptions.AGENT_AUTOSTART + "=false"),new VMOption("-D" + CreateEnsembleOptions.ZOOKEEPER_SERVER_PORT + "=2182"),new VMOption("-D" + CreateEnsembleOptions.ZOOKEEPER_SERVER_CONNECTION_PORT + "=2182")};
