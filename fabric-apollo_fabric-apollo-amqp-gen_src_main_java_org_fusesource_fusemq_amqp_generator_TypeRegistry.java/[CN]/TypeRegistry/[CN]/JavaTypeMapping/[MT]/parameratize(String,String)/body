{
  if (generic != null) {
    if (generics != null && generics.length > 0) {
      boolean first=true;
      for (      String g : generics) {
        if (!first) {
          str+=", " + g;
        }
 else {
          first=false;
          str+="<" + g;
        }
      }
      return str + ">";
    }
 else {
      return javaType + generic;
    }
  }
  return str;
}
