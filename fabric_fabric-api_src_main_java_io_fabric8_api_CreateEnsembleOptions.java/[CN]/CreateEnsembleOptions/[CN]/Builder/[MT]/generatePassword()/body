{
  StringBuilder password=new StringBuilder();
  for (int i=0; i < 16; i++) {
    long l=Math.round(Math.floor(Math.random() * (26 * 2 + 10)));
    if (l < 10) {
      password.append((char)('0' + l));
    }
 else     if (l < 36) {
      password.append((char)('A' + l - 10));
    }
 else {
      password.append((char)('a' + l - 36));
    }
  }
  return password.toString();
}
