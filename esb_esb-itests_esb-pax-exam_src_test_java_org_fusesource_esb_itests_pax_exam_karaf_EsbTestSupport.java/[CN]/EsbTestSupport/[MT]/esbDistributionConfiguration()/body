{
  return new DefaultCompositeOption(new Option[]{karafDistributionConfiguration().frameworkUrl(maven().groupId(GROUP_ID).artifactId(ARTIFACT_ID).versionAsInProject().type("zip")).karafVersion(getKarafVersion()).name("JBoss Fuse Distro").unpackDirectory(new File("target/paxexam/unpack/")),useOwnExamBundlesStartLevel(50),editConfigurationFilePut("etc/config.properties","karaf.startlevel.bundle","50"),mavenBundle("org.fusesource.tooling.testing","pax-exam-karaf",MavenUtils.getArtifactVersion("org.fusesource.tooling.testing","pax-exam-karaf"))});
}
