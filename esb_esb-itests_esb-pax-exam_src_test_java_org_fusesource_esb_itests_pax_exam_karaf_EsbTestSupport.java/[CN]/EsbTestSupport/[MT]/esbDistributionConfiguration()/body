{
  return new DefaultCompositeOption(new Option[]{karafDistributionConfiguration().frameworkUrl(maven().groupId(GROUP_ID).artifactId(ARTIFACT_ID).versionAsInProject().type("tar.gz")).karafVersion(getKarafVersion()).name("Fuse ESB Distro").unpackDirectory(new File("target/paxexam/unpack/")),useOwnExamBundlesStartLevel(40),editConfigurationFilePut("etc/config.properties","karaf.startlevel.bundle","35"),mavenBundle("org.fusesource.tooling.testing","pax-exam-karaf",MavenUtils.getArtifactVersion("org.fusesource.tooling.testing","pax-exam-karaf"))});
}
