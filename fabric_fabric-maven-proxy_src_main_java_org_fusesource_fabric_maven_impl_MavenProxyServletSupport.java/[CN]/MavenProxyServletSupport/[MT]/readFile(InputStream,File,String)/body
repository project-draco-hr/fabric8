{
  File tmpFile=null;
  FileOutputStream fos=null;
  try {
    tmpFile=new File(tempLocation,name);
    if (tmpFile.exists()) {
      tmpFile.delete();
    }
    fos=new FileOutputStream(tmpFile);
    int length=0;
    byte buffer[]=new byte[4096];
    while ((length=is.read(buffer)) != -1) {
      fos.write(buffer,0,length);
    }
  }
 catch (  Exception ex) {
  }
 finally {
    try {
      fos.flush();
    }
 catch (    Exception ex) {
    }
    try {
      fos.close();
    }
 catch (    Exception ex) {
    }
  }
  return tmpFile;
}
