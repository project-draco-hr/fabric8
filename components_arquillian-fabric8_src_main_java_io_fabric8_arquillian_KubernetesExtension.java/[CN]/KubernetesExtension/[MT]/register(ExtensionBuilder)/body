{
  builder.observer(Configuration.class).observer(Configurer.class).observer(ClientCreator.class).observer(ControllerCreator.class).observer(LoggerFactory.class).observer(SuiteListener.class).observer(SessionListener.class);
  builder.service(ResourceProvider.class,ClientResourceProvider.class).service(ResourceProvider.class,ControllerResourceProvider.class).service(ResourceProvider.class,PodListResourceProvider.class).service(ResourceProvider.class,ReplicationControllerListResourceProvider.class).service(ResourceProvider.class,ServiceListResourceProvider.class).service(ResourceProvider.class,SessionResourceProvider.class);
}
