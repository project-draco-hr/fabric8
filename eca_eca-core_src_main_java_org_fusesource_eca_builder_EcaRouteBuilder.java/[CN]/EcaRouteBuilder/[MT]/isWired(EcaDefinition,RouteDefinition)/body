{
  for (  ProcessorDefinition pd : routeDefinition.getOutputs()) {
    if (pd instanceof ToDefinition) {
      ToDefinition td=(ToDefinition)pd;
      String uri=td.getUriOrRef();
      if (uri != null && uri.indexOf(ecaDefinition.getUri()) >= 0) {
        return true;
      }
    }
  }
  return false;
}
