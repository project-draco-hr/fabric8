{
  if (profiles == null) {
    return "";
  }
  int iMax=profiles.length - 1;
  if (iMax == -1) {
    return "";
  }
  StringBuilder b=new StringBuilder();
  for (int i=0, j=profiles.length; i < j; i++) {
    b.append(profiles[i].getName());
    if (i + 1 < j) {
      b.append(", ");
    }
  }
  return b.toString();
}
