{
  return new StringBuilder().append(m_group.replaceAll(GROUP_SEPARATOR,FILE_SEPARATOR)).append(FILE_SEPARATOR).append(m_artifact).append(FILE_SEPARATOR).append(version).append(FILE_SEPARATOR).append(m_artifact).append(VERSION_SEPARATOR).append(getSnapshotVersion(version,timestamp,buildnumber)).append(m_fullClassifier).append(TYPE_SEPARATOR).append(m_type).toString();
}
