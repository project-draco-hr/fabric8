{
  long bundleId=bundle.getBundleId();
  addToMap(factoryPidToBundleIds,bundleId,info.getFactoryPids());
  addToMap(pidToBundleIds,bundleId,info.getPids());
  String[] localeArray=info.getLocales();
  if (localeArray != null) {
    for (    String locale : localeArray) {
      locales.add(locale);
    }
  }
}
