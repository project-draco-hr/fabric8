{
  StringBuffer ret=new StringBuffer();
  ret.append("AmqpMessage{");
  if (header != null) {
    ret.append("header=");
    ret.append(header);
    ret.append(" ");
  }
  if (properties != null) {
    ret.append("properties=");
    ret.append(properties);
    ret.append(" ");
  }
  ret.append("fragments=");
  ret.append(fragments);
  if (footer != null) {
    ret.append(" footer=");
    ret.append(footer);
  }
  ret.append("}");
  return ret.toString();
}
