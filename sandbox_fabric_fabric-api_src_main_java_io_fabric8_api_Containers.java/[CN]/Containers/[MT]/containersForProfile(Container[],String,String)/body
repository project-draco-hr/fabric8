{
  List<Container> answer=new ArrayList<Container>();
  if (profileId != null) {
    for (    Container c : containers) {
      String currentId=c.getVersionId();
      if (currentId != null && currentId.equals(versionId) && containerHasProfile(c,profileId)) {
        answer.add(c);
      }
    }
  }
  return answer;
}
