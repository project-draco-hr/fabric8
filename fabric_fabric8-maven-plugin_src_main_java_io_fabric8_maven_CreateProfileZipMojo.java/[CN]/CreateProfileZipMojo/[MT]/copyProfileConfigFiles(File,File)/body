{
  File[] files=profileConfigDir.listFiles();
  if (files != null) {
    profileBuildDir.mkdirs();
    for (    File file : files) {
      File outFile=new File(profileBuildDir,file.getName());
      if (file.isDirectory()) {
        copyProfileConfigFiles(file,outFile);
      }
 else {
        Files.copy(file,outFile);
      }
    }
  }
}
