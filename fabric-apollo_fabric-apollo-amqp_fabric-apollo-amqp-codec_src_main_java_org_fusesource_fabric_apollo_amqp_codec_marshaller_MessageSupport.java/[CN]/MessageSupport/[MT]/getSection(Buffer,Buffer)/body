{
  int position=buffer.indexOf(descriptor);
  if (position == -1) {
    return null;
  }
  DataByteArrayInputStream in=new DataByteArrayInputStream(buffer);
  while (position != -1) {
    try {
      in.setPos(position);
      return (T)TypeReader.read(in);
    }
 catch (    Exception e) {
    }
    position=buffer.indexOf(descriptor,position + 1);
  }
  return null;
}
