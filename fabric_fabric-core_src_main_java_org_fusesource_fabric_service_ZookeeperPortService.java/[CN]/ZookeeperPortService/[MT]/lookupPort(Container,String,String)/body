{
  int port=0;
  String path=ZkPath.PORTS_CONTAINER_PID_KEY.getPath(container.getId(),pid,key);
  try {
    if (exists(curator.get(),path) != null) {
      port=Integer.parseInt(getStringData(curator.get(),path));
    }
  }
 catch (  Exception ex) {
    throw new FabricException(ex);
  }
  return port;
}
