{
  builder.add(platform);
  platform.setValueChoices(Arrays.asList(platforms));
  platform.setDefaultValue(new Callable<String>(){
    @Override public String call() throws Exception {
      if (isPlatform("windows")) {
        return "Jube";
      }
 else {
        return "Docker";
      }
    }
  }
);
  builder.add(group);
}
