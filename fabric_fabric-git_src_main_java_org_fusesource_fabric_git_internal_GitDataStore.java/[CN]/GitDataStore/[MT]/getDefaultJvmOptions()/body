{
  assertValid();
  try {
    if (getCurator().getZookeeperClient().isConnected() && exists(getCurator(),JVM_OPTIONS_PATH) != null) {
      return getStringData(getTreeCache(),JVM_OPTIONS_PATH);
    }
 else {
      return "";
    }
  }
 catch (  Exception e) {
    throw new FabricException(e);
  }
}
