{
  DependencyTreeResult node=collectDependencies("test-override-spring.pom");
  assertShareFilter(node,"commons-logging","commons-logging",false,"");
  assertShareFilter(node,"commons-logging","commons-logging",true,"commons-*","commons-*:*","*:commons-*","commons-logging:!foo","!foo:*","*","commons-logging:*","*:*","*:commons-logging","commons-logging:commons-logging","foo:bar commons-logging:commons-logging","foo:bar commons-logging:commons-logging xyz:bar","commons-logging:commons-logging foo:bar ");
  assertShareFilter(node,"org.springframework","spring-core",true,"*","org.springframework:*","org.springframework:spring-core");
}
