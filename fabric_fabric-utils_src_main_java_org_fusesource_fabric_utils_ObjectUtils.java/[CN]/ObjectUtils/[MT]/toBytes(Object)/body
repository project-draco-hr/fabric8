{
  byte[] result=null;
  if (object instanceof byte[]) {
    return (byte[])object;
  }
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  ObjectOutputStream oos=null;
  try {
    oos=new ObjectOutputStream(baos);
    oos.writeObject(object);
    result=baos.toByteArray();
  }
 catch (  IOException e) {
  }
 finally {
    if (oos != null) {
      try {
        oos.close();
      }
 catch (      IOException e) {
      }
    }
    if (baos != null) {
      try {
        baos.close();
      }
 catch (      IOException e) {
      }
    }
  }
  return result;
}
