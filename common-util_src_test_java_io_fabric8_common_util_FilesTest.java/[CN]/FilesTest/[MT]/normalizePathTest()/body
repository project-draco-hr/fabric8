{
  Assert.assertEquals("/some/path",Files.normalizePath("\\some\\path",'\\','/'));
  Assert.assertEquals("/some/path",Files.normalizePath("/some/path",'\\','/'));
  Assert.assertEquals("\\some\\path",Files.normalizePath("/some/path",'/','\\'));
  Assert.assertEquals("\\some\\path",Files.normalizePath("\\some\\path",'/','\\'));
}
