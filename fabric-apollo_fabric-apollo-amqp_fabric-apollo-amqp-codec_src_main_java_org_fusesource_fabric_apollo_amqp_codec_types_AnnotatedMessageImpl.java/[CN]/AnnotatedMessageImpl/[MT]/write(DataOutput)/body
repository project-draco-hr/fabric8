{
  if (header != null) {
    header.write(out);
  }
  if (deliveryAnnotations != null) {
    deliveryAnnotations.write(out);
  }
  if (messageAnnotations != null) {
    messageAnnotations.write(out);
  }
  if (message != null) {
    message.write(out);
  }
  if (footer != null) {
    footer.write(out);
  }
}
