{
  assertPathSplit("");
  assertPathSplit("/");
  assertPathSplit("foo","foo");
  assertPathSplit("/foo","foo");
  assertPathSplit("/foo/","foo");
  assertPathSplit("/foo?cheese=bar","foo");
  assertPathSplit("foo/bar","foo","bar");
  assertPathSplit("/foo/bar","foo","bar");
  assertPathSplit("/foo/bar/","foo","bar");
}
