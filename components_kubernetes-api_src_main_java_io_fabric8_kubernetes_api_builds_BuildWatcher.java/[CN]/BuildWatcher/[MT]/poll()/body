{
  boolean foundBuild=false;
  BuildList buildList=kubernetes.builds().inNamespace(namespace).list();
  if (buildList != null) {
    List<Build> items=buildList.getItems();
    if (items != null) {
      for (      Build build : items) {
        buildPolled(build);
        foundBuild=true;
      }
    }
  }
  if (foundBuild) {
    loading=false;
  }
}
