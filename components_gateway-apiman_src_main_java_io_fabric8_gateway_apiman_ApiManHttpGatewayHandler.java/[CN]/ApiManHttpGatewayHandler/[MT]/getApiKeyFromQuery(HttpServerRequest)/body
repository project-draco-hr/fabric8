{
  String queryString=request.query();
  if (queryString == null) {
    return null;
  }
  int idx=queryString.indexOf("apikey=");
  if (idx >= 0) {
    int endIdx=queryString.indexOf('&',idx);
    if (endIdx == -1) {
      endIdx=queryString.length();
    }
    return queryString.substring(idx + 7,endIdx);
  }
 else {
    return null;
  }
}
