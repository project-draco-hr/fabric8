{
  if (jmxUser == null) {
    log.debug("Prompting user for jmx login");
    jmxUser=ShellUtils.readLine(session,"Jmx Login for " + parent + ": ",false);
    System.out.println();
  }
  if (jmxPassword == null) {
    jmxPassword=ShellUtils.readLine(session,"Jmx Password for " + parent + ": ",true);
    System.out.println();
  }
}
