{
  String brokerName=getBrokerName();
  ObjectName query=new ObjectName("org.apache.activemq:type=Broker,brokerName=" + brokerName + ",connector=clientConnectors,connectorName="+ connectorName+ ",connectionViewType=clientId,connectionName=*");
  Set<ObjectName> queryResult=queryNames(query,null);
  Collection<String> result=new ArrayList<String>(queryResult.size());
  for (  ObjectName on : queryResult) {
    String name=on.getKeyProperty("connectionName").replace('_',':');
    result.add(name);
  }
  return result;
}
