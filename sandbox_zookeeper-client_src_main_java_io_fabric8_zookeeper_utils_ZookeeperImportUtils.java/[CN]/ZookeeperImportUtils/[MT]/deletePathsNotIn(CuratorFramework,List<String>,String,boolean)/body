{
  List<String> zkPaths=curator.getChildren().forPath(target);
  for (  String path : zkPaths) {
    path="/" + path;
    if (!paths.contains(path)) {
      if (!dryRun) {
        curator.delete().guaranteed().forPath(path);
      }
 else {
        LOG.debug("Deleting path {} and everything under it",path);
      }
    }
  }
}
