{
  return new Option[]{new DefaultCompositeOption(fabricDistributionConfiguration()),KarafDistributionOption.editConfigurationFilePut("etc/system.properties","karaf.name","myroot"),KarafDistributionOption.editConfigurationFilePut("etc/system.properties","fabric.version",MavenUtils.getArtifactVersion("io.fabric8","fabric8-karaf"))};
}
