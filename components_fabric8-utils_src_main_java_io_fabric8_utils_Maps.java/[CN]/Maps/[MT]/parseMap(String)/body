{
  Map<String,String> answer=new HashMap<>();
  if (Strings.isNotBlank(text)) {
    String[] entries=text.split(",");
    if (entries != null) {
      for (      String entry : entries) {
        String[] values=entry.split("=",2);
        if (values != null && values.length == 2) {
          answer.put(values[0],values[1]);
        }
      }
    }
  }
  return answer;
}
