{
  String[] itfs;
  if (val instanceof String) {
    itfs=new String[]{(String)val};
  }
 else   if (val instanceof Collection) {
    itfs=(String[])((Collection)val).toArray(new String[0]);
  }
 else   if (val instanceof String[]) {
    itfs=(String[])val;
  }
 else {
    itfs=null;
  }
  return itfs;
}
