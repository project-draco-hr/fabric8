{
  java.util.List<String> paths=getPaths();
  for (  String p : paths) {
    if (display) {
      byte[] data=getZooKeeper().getData(p);
      if (data != null) {
        System.out.printf("%s = %s\n",p,new String(data));
      }
 else {
        System.out.println(p);
      }
    }
 else {
      System.out.println(p);
    }
  }
}
