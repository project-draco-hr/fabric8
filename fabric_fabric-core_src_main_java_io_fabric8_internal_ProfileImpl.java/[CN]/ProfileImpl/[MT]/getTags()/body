{
  List<String> answer=getContainerConfigList(this,ConfigListType.TAGS);
  if (answer == null || answer.size() == 0) {
    answer=new ArrayList<>();
    String id=getId();
    String[] paths=id.split("-");
    if (paths != null) {
      for (int i=0, last=paths.length - 1; i < last; i++) {
        answer.add(paths[i]);
      }
    }
  }
  return answer;
}
