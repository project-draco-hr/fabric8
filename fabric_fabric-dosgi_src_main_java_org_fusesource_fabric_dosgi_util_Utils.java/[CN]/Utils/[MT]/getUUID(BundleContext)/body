{
  String uuid=bundleContext.getProperty(FRAMEWORK_UUID);
  if (uuid == null) {
synchronized (FRAMEWORK_UUID) {
      uuid=bundleContext.getProperty(FRAMEWORK_UUID);
      if (uuid == null) {
        uuid=UuidGenerator.getUUID();
        System.setProperty(FRAMEWORK_UUID,uuid);
      }
    }
  }
  return uuid;
}
