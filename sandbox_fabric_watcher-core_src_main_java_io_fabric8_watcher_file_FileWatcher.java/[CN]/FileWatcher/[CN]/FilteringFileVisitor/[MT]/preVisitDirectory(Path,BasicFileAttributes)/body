{
  if (Thread.interrupted()) {
    throw new InterruptedIOException();
  }
  if (dirMatcher != null) {
    Path rel=root.relativize(dir);
    if (!"".equals(rel.toString()) && !dirMatcher.matches(rel)) {
      return FileVisitResult.SKIP_SUBTREE;
    }
  }
  watch(dir);
  return FileVisitResult.CONTINUE;
}
