{
  String answer="";
  String[] lines=text.split("\r?\n");
  for (  String line : lines) {
    String l=line.trim();
    if (!l.startsWith("##") && !l.startsWith("#set(")) {
      if (line.contains("${D}")) {
        line=line.replaceAll("\\$\\{D\\}","\\$");
      }
      answer=answer.concat(line);
      answer=answer.concat("\n");
    }
  }
  return answer;
}
