{
  try {
    String url=toUrlString(path);
    LOG.info("Removing Dozer Mapping file " + url);
    WatcherDozerTypeConverterLoader loader=loaders.remove(url);
    if (url != null) {
      loader.remove();
    }
  }
 catch (  Exception e) {
    LOG.warn("Ignored path " + path + " due to: "+ e,e);
  }
}
