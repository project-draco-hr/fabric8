{
  while (keepRunning) {
    if (!url.equals(curator.getZookeeperClient().getCurrentConnectionString())) {
      return curator.getZookeeperClient().getCurrentConnectionString();
    }
 else {
      try {
        Thread.sleep(1000L);
      }
 catch (      Exception ex) {
      }
    }
  }
  return null;
}
