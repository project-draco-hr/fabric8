{
  assertValid();
  gitOperation(new GitOperation<Void>(){
    public Void call(    Git git,    GitContext context) throws Exception {
      removeVersion(version);
      GitHelpers.removeBranch(git,version);
      git.push().setTimeout(gitTimeout).setCredentialsProvider(getCredentialsProvider()).setRefSpecs(new RefSpec().setSource(null).setDestination("refs/heads/" + version)).call();
      return null;
    }
  }
);
}
