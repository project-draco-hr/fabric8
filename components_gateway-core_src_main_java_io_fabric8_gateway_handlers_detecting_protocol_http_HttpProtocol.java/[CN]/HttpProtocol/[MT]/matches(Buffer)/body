{
  return startsWith(header,0,GET.toBuffer()) || startsWith(header,0,HEAD.toBuffer()) || startsWith(header,0,POST.toBuffer())|| startsWith(header,0,PUT.toBuffer())|| startsWith(header,0,DELETE.toBuffer())|| startsWith(header,0,OPTIONS.toBuffer())|| startsWith(header,0,TRACE.toBuffer())|| startsWith(header,0,CONNECT.toBuffer());
}
