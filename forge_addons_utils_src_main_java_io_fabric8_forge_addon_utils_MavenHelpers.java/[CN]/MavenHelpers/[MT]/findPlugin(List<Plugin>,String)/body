{
  if (plugins != null) {
    for (    Plugin plugin : plugins) {
      String groupId=plugin.getGroupId();
      if (Strings.isNullOrBlank(groupId) || Objects.equal(groupId,mavenPluginsGroupId)) {
        if (Objects.equal(artifactId,plugin.getArtifactId())) {
          return plugin;
        }
      }
    }
  }
  return null;
}
