{
  if (project != null) {
    MavenPluginFacet pluginFacet=project.getFacet(MavenPluginFacet.class);
    if (pluginFacet != null) {
      List<MavenPlugin> plugins=pluginFacet.listConfiguredPlugins();
      if (plugins != null) {
        for (        MavenPlugin plugin : plugins) {
          Coordinate coordinate=plugin.getCoordinate();
          if (coordinate != null) {
            if (Objects.equal(groupId,coordinate.getGroupId()) && Objects.equal(artifactId,coordinate.getArtifactId())) {
              return plugin;
            }
          }
        }
      }
    }
  }
  return null;
}
