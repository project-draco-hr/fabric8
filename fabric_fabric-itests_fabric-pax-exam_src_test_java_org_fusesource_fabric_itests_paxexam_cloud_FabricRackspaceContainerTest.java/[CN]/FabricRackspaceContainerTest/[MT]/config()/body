{
  return new Option[]{new DefaultCompositeOption(fabricDistributionConfiguration()),copySystemProperty("fabricitest.rackspace.identity"),copySystemProperty("fabricitest.rackspace.credential"),copySystemProperty("fabricitest.rackspace.image"),copySystemProperty("fabricitest.rackspace.location"),copySystemProperty("fabricitest.rackspace.user"),editConfigurationFileExtend("etc/config.properties","org.osgi.framework.executionenvironment","JavaSE-1.7,JavaSE-1.6,JavaSE-1.5"),scanFeatures("jclouds","jclouds-compute").start()};
}
