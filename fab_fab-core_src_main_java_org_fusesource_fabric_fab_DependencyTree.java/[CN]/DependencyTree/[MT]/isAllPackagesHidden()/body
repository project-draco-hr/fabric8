{
  try {
    Set<String> nonMetaPackages=getPackages();
    for (    String key : new ArrayList<String>(nonMetaPackages)) {
      if (key.startsWith("META-INF")) {
        nonMetaPackages.remove(key);
      }
    }
    return hiddenPackages.containsAll(nonMetaPackages);
  }
 catch (  IOException e) {
    return true;
  }
}
