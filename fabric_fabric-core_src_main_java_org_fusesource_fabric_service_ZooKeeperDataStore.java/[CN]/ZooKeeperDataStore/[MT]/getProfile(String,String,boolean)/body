{
  assertValid();
  try {
    String path=ZkProfiles.getPath(version,profile);
    if (getTreeCache().getCurrentData(path) == null) {
      if (!create) {
        return null;
      }
 else {
        createProfile(version,profile);
        return profile;
      }
    }
    return profile;
  }
 catch (  Exception e) {
    throw FabricException.launderThrowable(e);
  }
}
