{
  assertValid();
  try {
    String path=ZkProfiles.getPath(version,profile);
    Properties props=getProperties(getCurator(),path);
    if (value != null) {
      props.setProperty(key,value);
    }
 else {
      props.remove(key);
    }
    setProperties(getCurator(),path,props);
  }
 catch (  Exception e) {
    throw FabricException.launderThrowable(e);
  }
}
