{
  assertValid();
  try {
    if (getCurator() != null && getCurator().getZookeeperClient().isConnected() && getTreeCache().getCurrentData(ZkPath.CONFIG_VERSION.getPath(name)) == null) {
      return false;
    }
    return true;
  }
 catch (  FabricException e) {
    throw e;
  }
catch (  Exception e) {
    throw new FabricException(e);
  }
}
