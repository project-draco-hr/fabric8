{
  assertEquals("Two context policies were expected",2,policySet.policiesWithExactScopeElements(ScopeElement.BUNDLE,ScopeElement.CONTEXT).size());
  assertEquals("One excluding context policies was expected",1,policySet.policiesWithExactScopeElements(ScopeElement.BUNDLE,ScopeElement.CONTEXT).queryAllExclusions().size());
}
