{
  assertEquals("Policies that apply to event 'CREATED' should be 2",2,policySet.filtersForScopeElement(ScopeElement.EVENT).filtersOfType(EnumerationFilter.class).allMatchesFor("CREATED").size());
  assertEquals("Policies that apply to event 'CREATED' should be 2",2,policySet.filtersForScopeElement(ScopeElement.EVENT).filtersOfType(EnumerationFilter.class).countMatches("CREATED"));
  FilterSet<ExpressionFilter> filterSet=policySet.filtersForScopeElement(ScopeElement.CONTEXT).filtersOfType(ExpressionFilter.class);
  assertEquals("Policies for camelContext1 should be 1",1,Sets.filter(filterSet,new Predicate<ExpressionFilter>(){
    @Override public boolean apply(    ExpressionFilter ef){
      return ef.getExpression().contains("camelContext1");
    }
  }
).size());
}
