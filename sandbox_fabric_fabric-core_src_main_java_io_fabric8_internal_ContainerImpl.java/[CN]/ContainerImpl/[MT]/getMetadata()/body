{
  if (metadata == null) {
    metadata=getMetadata(getClass().getClassLoader());
    if (metadata == null) {
      for (      Class<?> type : fabricService.getSupportedCreateContainerMetadataTypes()) {
        metadata=getMetadata(type.getClassLoader());
        if (metadata != null) {
          break;
        }
      }
    }
  }
  return metadata;
}
