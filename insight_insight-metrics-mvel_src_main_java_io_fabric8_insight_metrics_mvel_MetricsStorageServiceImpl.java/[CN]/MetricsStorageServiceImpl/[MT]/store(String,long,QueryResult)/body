{
  String output=null;
  try {
    output=renderer.render(qrs);
  }
 catch (  Exception e) {
    LOG.warn("Failed to render " + qrs + " to JSON: "+ e,e);
  }
  if (output == null || output.trim().isEmpty()) {
    return;
  }
  String name=Metrics.metricId(type,qrs);
  storageService.store(name,timestamp,output);
}
