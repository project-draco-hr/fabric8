{
  String address="";
  String path=null;
  try {
    path=getPath(container,oName,address,true);
    removeZkPath(path);
    path=getPath(container,oName,address,false);
    removeZkPath(path);
  }
 catch (  Exception e) {
    LOGGER.error("Failed to unregister API endpoint at {}.",path,e);
  }
}
