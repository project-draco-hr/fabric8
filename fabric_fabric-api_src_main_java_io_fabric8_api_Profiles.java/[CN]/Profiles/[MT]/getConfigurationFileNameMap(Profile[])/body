{
  Map<String,String> answer=new TreeMap<String,String>();
  for (  Profile profile : profiles) {
    String id=profile.getId();
    Set<String> files=profile.getConfigurationFileNames();
    for (    String file : files) {
      if (!answer.containsKey(file)) {
        answer.put(file,id);
      }
    }
  }
  return answer;
}
