{
  if (map == null) {
    out.writeInt(-1);
  }
 else {
    out.writeInt(map.size());
    for (Iterator iter=map.keySet().iterator(); iter.hasNext(); ) {
      String name=(String)iter.next();
      out.writeUTF(name);
      Object value=map.get(name);
      marshalPrimitive(out,value);
    }
  }
}
