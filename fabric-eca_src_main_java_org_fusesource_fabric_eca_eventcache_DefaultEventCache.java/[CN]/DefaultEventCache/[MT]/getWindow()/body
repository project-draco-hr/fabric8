{
  pruneCache(getEventClock().currentTimeMillis());
  List<T> result=new ArrayList<T>(size);
  try {
    lock.readLock().lock();
    CacheItemImpl node=root;
    while (node != null) {
      result.add(node.item);
      node=node.getNext();
    }
  }
  finally {
    lock.readLock().unlock();
  }
  return result;
}
