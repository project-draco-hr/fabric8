{
  if (packages == null) {
    if (getExtension().equals("jar") || getExtension().equals("zip")) {
      aQute.lib.osgi.Jar jar=new aQute.lib.osgi.Jar(getJarFile());
      try {
        packages=new HashSet<String>(jar.getPackages());
      }
  finally {
        jar.close();
      }
    }
 else {
      return Collections.emptySet();
    }
  }
  return packages;
}
