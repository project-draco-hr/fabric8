{
  if (m_string == null) {
switch (m_op) {
case AND:
      m_string="(&" + toString((List)m_value) + ")";
    break;
case OR:
  m_string="(|" + toString((List)m_value) + ")";
break;
case NOT:
m_string="(!" + toString((List)m_value) + ")";
break;
case EQ:
m_string="(" + m_name + "="+ toEncodedString(m_value)+ ")";
break;
case LTE:
m_string="(" + m_name + "<="+ toEncodedString(m_value)+ ")";
break;
case GTE:
m_string="(" + m_name + ">="+ toEncodedString(m_value)+ ")";
break;
case SUBSTRING:
m_string="(" + m_name + "="+ unparseSubstring((List<String>)m_value)+ ")";
break;
case PRESENT:
m_string="(" + m_name + "=*)";
break;
case APPROX:
m_string="(" + m_name + "~="+ toEncodedString(m_value)+ ")";
break;
default :
throw new IllegalStateException();
}
}
return m_string;
}
