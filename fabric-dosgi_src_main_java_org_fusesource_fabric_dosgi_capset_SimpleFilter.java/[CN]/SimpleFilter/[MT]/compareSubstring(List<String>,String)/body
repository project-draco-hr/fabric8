{
  boolean result=true;
  int len=pieces.size();
  if (len == 1) {
    return s.equals(pieces.get(0));
  }
  int index=0;
  for (int i=0; i < len; i++) {
    String piece=pieces.get(i);
    if (i == 0) {
      if (!s.startsWith(piece)) {
        result=false;
        break;
      }
    }
    if (i == (len - 1)) {
      result=s.endsWith(piece);
      break;
    }
    if ((i > 0) && (i < (len - 1))) {
      index=s.indexOf(piece,index);
      if (index < 0) {
        result=false;
        break;
      }
    }
    index+=piece.length();
  }
  return result;
}
