{
  if (o instanceof String) {
    String s=(String)o;
    StringBuffer sb=new StringBuffer();
    for (int i=0; i < s.length(); i++) {
      char c=s.charAt(i);
      if ((c == '\\') || (c == '(') || (c == ')')|| (c == '*')) {
        sb.append('\\');
      }
      sb.append(c);
    }
    o=sb.toString();
  }
  return o.toString();
}
