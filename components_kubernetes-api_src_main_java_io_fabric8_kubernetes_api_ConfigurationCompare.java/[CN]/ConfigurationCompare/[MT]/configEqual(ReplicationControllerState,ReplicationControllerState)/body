{
  if (entity1 == entity2) {
    return true;
  }
 else   if (entity1 == null || entity2 == null) {
    return false;
  }
  return configEqual(entity1.getReplicas(),entity2.getReplicas()) || configEqual(entity1.getReplicaSelector(),entity2.getReplicaSelector()) || configEqual(entity1.getPodTemplate(),entity2.getPodTemplate());
}
