{
  if (entity1 == entity2) {
    return true;
  }
 else   if (entity1 == null || entity2 == null) {
    return false;
  }
  return configEqual(entity1.getEmptyDir(),entity2.getEmptyDir()) && configEqual(entity1.getGitRepo(),entity2.getGitRepo()) && configEqual(entity1.getGlusterfs(),entity2.getGlusterfs())&& configEqual(entity1.getHostDir(),entity2.getHostDir())&& configEqual(entity1.getIscsi(),entity2.getIscsi())&& configEqual(entity1.getNfs(),entity2.getNfs())&& configEqual(entity1.getPersistentDisk(),entity2.getPersistentDisk())&& configEqual(entity1.getSecret(),entity2.getSecret());
}
