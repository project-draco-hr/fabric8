{
  if (entity1 == entity2) {
    return true;
  }
 else   if (entity1 == null || entity2 == null) {
    return false;
  }
  return configEqual(entity1.getAlways(),entity2.getAlways()) || configEqual(entity1.getNever(),entity2.getNever()) || configEqual(entity1.getOnFailure(),entity2.getOnFailure());
}
