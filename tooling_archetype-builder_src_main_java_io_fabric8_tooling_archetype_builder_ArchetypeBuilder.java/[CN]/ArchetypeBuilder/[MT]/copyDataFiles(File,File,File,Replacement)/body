{
  if (srcDir.isFile()) {
    copyFile(srcDir,outDir,replaceFn);
  }
 else {
    outDir.mkdirs();
    String[] names=srcDir.list();
    if (names != null) {
      for (      String name : names) {
        copyDataFiles(projectDir,new File(srcDir,name),new File(outDir,name),replaceFn);
      }
    }
  }
}
