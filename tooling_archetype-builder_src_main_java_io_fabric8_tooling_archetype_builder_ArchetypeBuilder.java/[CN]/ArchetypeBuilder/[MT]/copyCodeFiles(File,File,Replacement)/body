{
  if (rootPackage.isFile()) {
    copyFile(rootPackage,outDir,replaceFn);
  }
 else {
    outDir.mkdirs();
    String[] names=rootPackage.list();
    if (names != null) {
      for (      String name : names) {
        copyCodeFiles(new File(rootPackage,name),new File(outDir,name),replaceFn);
      }
    }
  }
}
