{
  List<Container> answer=new ArrayList<Container>();
  if (profileId != null) {
    for (    Container c : containers) {
      Version version=c.getVersion();
      if (version != null && version.getId().equals(versionId) && containerHasProfile(c,profileId)) {
        answer.add(c);
      }
    }
  }
  return answer;
}
