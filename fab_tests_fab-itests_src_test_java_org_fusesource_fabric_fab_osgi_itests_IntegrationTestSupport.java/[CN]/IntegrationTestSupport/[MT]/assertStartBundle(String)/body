{
  Bundle bundle=assertInstalledBundle(symbolicName);
  LOG.info("installed bundle: " + bundle + " is about to start");
  try {
    bundle.start();
    startedBundles.addLast(bundle);
  }
 catch (  BundleException e) {
    println("ERROR: " + e,e);
    throw e;
  }
  Thread.sleep(1000);
  return bundle;
}
