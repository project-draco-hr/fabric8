{
  for (  AmqpFragment fragment : list) {
    int sectionCode=fragment.getSectionCode().intValue();
switch (sectionCode) {
case HEADER:
      setHeader(fragment);
    break;
case PROPERTIES:
  setProperties(fragment);
break;
case FOOTER:
setFooter(fragment);
break;
default :
fragments.add(fragment);
}
}
}
