{
  FabricStatusDTO dto=fabricStatus();
  if (dto != null) {
    try {
      return getObjectMapper().writerWithDefaultPrettyPrinter().withType(FabricStatusDTO.class).writeValueAsString(dto);
    }
 catch (    JsonProcessingException e) {
      throw new RuntimeException("Error writing data as json",e);
    }
  }
 else {
