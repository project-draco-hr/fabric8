{
  if (table == null || jcoTable == null)   return;
  EStructuralFeature feature=table.eClass().getEStructuralFeature(ROW);
  @SuppressWarnings("unchecked") EList<Structure> records=(EList<Structure>)getValue(table,feature);
  for (  Structure row : records) {
    jcoTable.appendRow();
    fillJCoRecordFromStructure(row,(JCoRecord)jcoTable);
  }
}
