{
  checkFabricAvailable();
  Container[] containers=fabricService.getContainers();
  Version ver=null;
  if (version != null) {
    ver=fabricService.getVersion(version);
  }
  if (verbose) {
    printContainersVerbose(containers,ver,System.out);
  }
 else {
    printContainers(containers,ver,System.out);
  }
  return null;
}
