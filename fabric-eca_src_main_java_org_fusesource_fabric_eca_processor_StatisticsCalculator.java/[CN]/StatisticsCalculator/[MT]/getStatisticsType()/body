{
  String result="ALL";
  if (statisticsTypes != null && statisticsTypes.length > 0) {
    result="";
    for (int i=0; i < statisticsTypes.length; i++) {
      result+=statisticsTypes[i].name();
      if (i < (statisticsTypes.length - 1)) {
        result+=",";
      }
    }
  }
  return result;
}
