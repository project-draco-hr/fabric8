{
  List<String> answer=new ArrayList<String>();
  if (Strings.isNotBlank(parameterValue)) {
    String[] split=parameterValue.split("\\s");
    if (split != null) {
      for (      String text : split) {
        if (Strings.isNotBlank(text)) {
          answer.add(text);
        }
      }
    }
  }
  return answer;
}
