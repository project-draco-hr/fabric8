{
  if (initialized.compareAndSet(true,false)) {
    configuration.removeChangeListener(this);
    apmAgentContext.shutDown();
    try {
      Strategy s=this.strategy;
      if (s != null) {
        s.shutDown();
      }
    }
 catch (    Throwable e) {
      LOG.error("Failed to shutdown",e);
    }
  }
}
