{
  Set<Node> nodeSet=nodesClusterMap.get(clusterName);
  if (nodeSet != null) {
    for (    Node node : nodeSet) {
      ClusterAdminClient client=node.client().admin().cluster();
      NodesInfoResponse response=client.prepareNodesInfo().all().execute().actionGet();
      return response;
    }
  }
  return null;
}
