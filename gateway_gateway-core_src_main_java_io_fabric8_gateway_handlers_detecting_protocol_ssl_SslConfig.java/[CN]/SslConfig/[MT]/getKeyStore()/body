{
  if (keyStore == null) {
    if (storeType == null) {
      storeType="JKS";
    }
    if (keyStorePassword == null) {
      keyStorePassword="";
    }
    KeyStore store=KeyStore.getInstance(storeType);
    InputStream stream=keyStoreURL.openStream();
    try {
      store.load(stream,keyStorePassword.toCharArray());
    }
  finally {
      stream.close();
    }
    keyStore=store;
  }
  return keyStore;
}
