{
  for (  String expectedLine : expectedLines) {
    boolean found=false;
    for (    String line : lines) {
      if (line != null) {
        String trimmed=line.trim();
        if (trimmed.equals(expectedLine)) {
          found=true;
          break;
        }
      }
    }
    assertTrue("Did not find expected '" + expectedLine + "' in generated file "+ outputFile,found);
  }
}
