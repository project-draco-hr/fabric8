{
  String rootUrl=agent.getJmxUrl();
  if (rootUrl == null) {
    return null;
  }
  JMXConnector connector;
  try {
    connector=JMXConnectorFactory.connect(new JMXServiceURL(rootUrl),getEnvironmentCredentials());
  }
 catch (  IOException e) {
    throw new FabricException(e);
  }
  return connector;
}
