{
  setUriTemplate("/bar/{version}{contextPath}/",oldVersion);
  addQuickstartServices();
  assertTrue("Should have generated " + outputFile,outputFile.exists() && outputFile.isFile());
  List<String> lines=Files.readLines(outputFile);
  assertLinesContains(lines,"use_backend b_bar_1.0_cxf_crm if { path_beg /bar/1.0/cxf/crm/ }","use_backend b_bar_1.0_cxf_HelloWorld if { path_beg /bar/1.0/cxf/HelloWorld/ }","backend b_bar_1.0_cxf_crm","server resty  localhost:8182","backend b_bar_1.0_cxf_HelloWorld","server soapy  localhost:8183");
  LOG.info("About to reload the proxy command");
  gateway.reloadHaproxy();
  Thread.sleep(3000);
  LOG.info("Done!");
}
