{
  InputStream is=null;
  try {
    URL url=new URL(value.substring("checksum:".length()));
    is=url.openStream();
    return String.valueOf(ChecksumUtils.checksum(is));
  }
 catch (  Exception ex) {
    LOGGER.debug("Could not resolve placeholder",ex);
    return "0";
  }
 finally {
    Closeables.closeQuitely(is);
  }
}
