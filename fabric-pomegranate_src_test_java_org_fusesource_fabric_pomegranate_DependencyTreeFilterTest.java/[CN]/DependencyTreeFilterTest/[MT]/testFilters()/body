{
  DependencyTreeResult node=collectDependencies("test-override-spring.pom");
  assertFilterMatches(node,"commons-logging","commons-logging",false,"");
  assertFilterMatches(node,"commons-logging","commons-logging",true,"*","commons-logging:*","*:*","*:commons-logging","commons-logging:commons-logging","foo:bar commons-logging:commons-logging","foo:bar commons-logging:commons-logging xyz:bar","commons-logging:commons-logging foo:bar ");
  assertFilterMatches(node,"org.springframework","spring-core",true,"*","org.springframework:*","org.springframework:spring-core");
}
