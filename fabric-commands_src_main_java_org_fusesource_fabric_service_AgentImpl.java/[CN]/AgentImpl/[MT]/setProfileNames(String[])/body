{
  try {
    String version=getVersion();
    String node="/fabric/configs/versions/" + version + "/agents/"+ id;
    String str="";
    for (    String profile : profiles) {
      if (!str.isEmpty()) {
        str+=" ";
      }
      str+=profile;
    }
    zooKeeper.setData(node,str);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
