{
  node=createNode();
  try {
    workerCache=createWorkerCache();
    workerCache.getListenable().addListener(this);
    workerCache.start(true);
    ZooKeeperUtils.createDefault(curator,partitionPath,null);
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
  group.update(createNode());
  group.start();
}
