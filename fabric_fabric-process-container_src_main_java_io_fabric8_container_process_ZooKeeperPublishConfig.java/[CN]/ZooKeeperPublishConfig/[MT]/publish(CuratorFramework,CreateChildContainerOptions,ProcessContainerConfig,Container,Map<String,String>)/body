{
  LOG.info("Publishing to ZK path: " + path + " value "+ publishValue);
  if (!Strings.isNullOrBlank(path)) {
    try {
      ZooKeeperUtils.setData(curator,path,publishValue);
    }
 catch (    Exception e) {
      LOG.error("Failed to write ZK path " + path + " value "+ publishValue+ ". "+ e,e);
    }
  }
}
