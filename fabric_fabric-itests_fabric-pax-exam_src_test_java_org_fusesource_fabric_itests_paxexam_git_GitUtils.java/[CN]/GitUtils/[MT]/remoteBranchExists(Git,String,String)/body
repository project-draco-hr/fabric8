{
  List<Ref> list=git.branchList().setListMode(ListBranchCommand.ListMode.REMOTE).call();
  String fullName="refs/remotes/" + remote + "/"+ branch;
  boolean exists=false;
  for (  Ref ref : list) {
    String name=ref.getName();
    if (Objects.equal(name,fullName)) {
      exists=true;
    }
  }
  return exists;
}
