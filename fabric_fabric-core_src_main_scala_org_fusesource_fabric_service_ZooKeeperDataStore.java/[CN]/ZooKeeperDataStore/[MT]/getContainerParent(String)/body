{
  try {
    String parent=zk.getStringData(ZkPath.CONTAINER_PARENT.getPath(containerId));
    if (parent == null) {
      return "";
    }
    return parent.trim();
  }
 catch (  KeeperException.NoNodeException e) {
    return "";
  }
catch (  Throwable e) {
    throw new FabricException(e);
  }
}
