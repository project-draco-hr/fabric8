{
  try {
    String path=ZkProfiles.getPath(version,profile) + "/" + pid+ ".properties";
    if (exists(zk,path) == null) {
      return null;
    }
    byte[] data=zk.getData(path);
    return DataStoreHelpers.toMap(DataStoreHelpers.toProperties(data));
  }
 catch (  Exception e) {
    throw new FabricException(e);
  }
}
