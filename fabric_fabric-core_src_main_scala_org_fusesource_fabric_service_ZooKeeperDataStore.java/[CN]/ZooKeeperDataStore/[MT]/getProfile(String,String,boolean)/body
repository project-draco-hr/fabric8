{
  try {
    String path=ZkProfiles.getPath(version,profile);
    if (exists(curator,path) == null) {
      if (!create) {
        return null;
      }
 else {
        createProfile(version,profile);
        return profile;
      }
    }
    return profile;
  }
 catch (  Exception e) {
    throw new FabricException(e);
  }
}
