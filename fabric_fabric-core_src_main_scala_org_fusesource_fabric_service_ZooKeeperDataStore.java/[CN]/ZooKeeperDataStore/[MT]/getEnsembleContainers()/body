{
  List<String> containers=new ArrayList<String>();
  try {
    String ensemble=getStringData(curator,ZkPath.CONFIG_ENSEMBLE.getPath(getClusterId()));
    if (ensemble != null) {
      for (      String name : ensemble.trim().split(",")) {
        containers.add(name);
      }
    }
  }
 catch (  Exception e) {
    throw new FabricException(e);
  }
  return containers;
}
