{
  GitNode master=group.master();
  String masterUrl=master != null ? master.getUrl() : null;
  try {
    if (masterUrl != null) {
      gitService.notifyRemoteChanged(getSubstitutedData(curator,masterUrl));
    }
  }
 catch (  Exception e) {
    LOGGER.error("Failed to point origin to the new master.",e);
  }
}
