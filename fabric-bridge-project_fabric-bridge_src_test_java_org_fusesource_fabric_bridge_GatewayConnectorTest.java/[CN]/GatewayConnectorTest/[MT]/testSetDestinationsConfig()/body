{
  connector.afterPropertiesSet();
  connector.setDestinationsConfig(connector.getDestinationsConfig());
  connector.start();
  connector.setDestinationsConfig(connector.getDestinationsConfig());
  connector.stop();
  connector.setDestinationsConfig(connector.getDestinationsConfig());
  connector.destroy();
  connector.setDestinationsConfig(connector.getDestinationsConfig());
}
