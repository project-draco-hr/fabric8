{
  writer.newLine();
  writer.write(Utils.tab(indent) + "public " + bufferMapping+ " getBuffer(AmqpMarshaller marshaller) throws AmqpEncodingError{");
  writer.newLine();
  writer.write(Utils.tab(++indent) + "return this;");
  writer.newLine();
  writer.write(Utils.tab(--indent) + "}");
  writer.newLine();
  return indent;
}
