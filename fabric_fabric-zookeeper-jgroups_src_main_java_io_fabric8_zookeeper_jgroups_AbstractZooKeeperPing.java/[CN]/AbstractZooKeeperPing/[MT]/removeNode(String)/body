{
  try {
    curator.delete().forPath(path);
  }
 catch (  KeeperException.NoNodeException e) {
    if (log.isTraceEnabled()) {
      log.trace(String.format("Node '%s' already removed: %s",path,e));
    }
  }
catch (  Exception e) {
    log.error(String.format("Failed removing %s",path),e);
  }
}
