{
  this.id=id;
  this.logger=logger;
  namespacePrefix=Systems.getEnvVarOrSystemProperty("FABRIC8_NAMESPACE_PREFIX","itest-");
  namespace=namespacePrefix + id;
  namespaceDetails=new NamespaceBuilder().withNewMetadata().withName(namespace).addToLabels("provider","fabric8").addToLabels("component","integrationTest").addToLabels("framework","arquillian").withAnnotations(createAnnotations()).endMetadata().build();
}
