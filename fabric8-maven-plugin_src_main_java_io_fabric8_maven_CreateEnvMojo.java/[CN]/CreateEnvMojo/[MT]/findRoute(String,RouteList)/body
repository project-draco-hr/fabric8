{
  for (  Route route : routeList.getItems()) {
    RouteSpec spec=route.getSpec();
    if (spec != null) {
      ObjectReference to=spec.getTo();
      if (to != null) {
        String name=to.getName();
        if (serviceId.equals(name)) {
          return route;
        }
      }
    }
  }
  return null;
}
