{
  if (text == null) {
    return text;
  }
  StringBuilder buffer=new StringBuilder();
  int length=text.length();
  for (int i=0; i < length; i++) {
    char ch=text.charAt(i);
    if (ch == '"') {
      buffer.append("\\\"");
    }
 else {
      buffer.append(ch);
    }
  }
  return buffer.toString();
}
