{
  Option[] options=combine(Helper.getDefaultOptions(Helper.setLogLevel("INFO")),Helper.loadKarafStandardFeatures("obr","wrapper"),scanFeatures(maven().groupId("org.fusesource.fabric").artifactId("fabric-distro").type("xml").classifier("features").versionAsInProject(),"fabric-fab"),workingDirectory("target/paxrunner/core/"),waitForFrameworkStartup(),felix().version("3.0.2"),felixProvisionalApis());
