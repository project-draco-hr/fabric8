{
  this.executor.shutdown();
  try {
    this.executor.awaitTermination(5,TimeUnit.SECONDS);
  }
 catch (  InterruptedException e) {
  }
  this.executor.shutdownNow();
  try {
    this.executor.awaitTermination(5,TimeUnit.SECONDS);
  }
 catch (  InterruptedException e) {
  }
  this.mbeanServer.close();
  this.sender.close();
  if (globalGroup != null) {
    globalGroup.close();
  }
  if (hostGroup != null) {
    hostGroup.close();
  }
}
