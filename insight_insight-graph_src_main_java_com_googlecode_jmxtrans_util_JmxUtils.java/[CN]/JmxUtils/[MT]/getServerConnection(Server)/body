{
  JMXServiceURL url=new JMXServiceURL(server.getUrl());
  if (server.getProtocolProviderPackages() != null && server.getProtocolProviderPackages().contains("weblogic"))   return JMXConnectorFactory.connect(url,getWebLogicEnvironment(server));
 else   return JMXConnectorFactory.connect(url,getEnvironment(server));
}
