{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (name: ");
  result.append(name);
  result.append(", group: ");
  result.append(group);
  result.append(", description: ");
  result.append(description);
  result.append(')');
  return result.toString();
}
