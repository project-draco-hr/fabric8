{
  for (int i=0; i < MAX_ITERATIONS; i++) {
    for (    TestValues value : TestValues.values()) {
      System.out.println("TestLoad " + name + " running #"+ i);
      load(value);
      if (i % 2 == 0) {
        doSomethingElse();
      }
    }
  }
  System.out.println("TestLoad " + name + " done");
  done.set(true);
}
