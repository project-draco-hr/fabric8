{
  Object r=properties.get(propName);
  if (r == null) {
    return 0l;
  }
  try {
    return ((Long)r).longValue();
  }
 catch (  ClassCastException e) {
    IllegalArgumentException iae=new IllegalArgumentException("property value is not a Long: " + propName);
    iae.initCause(e);
    throw iae;
  }
}
