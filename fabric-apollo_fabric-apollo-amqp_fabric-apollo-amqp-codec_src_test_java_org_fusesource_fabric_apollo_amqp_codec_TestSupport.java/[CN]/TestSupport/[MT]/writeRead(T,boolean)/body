{
  long size=value.size();
  if (size < 80 && display) {
    System.out.printf("%s -> ",value);
  }
  byte b[]=write(value);
  if (size < 80 && display) {
    System.out.printf("%s -> ",string(b));
  }
  T rc=(T)read(b);
  if (size < 80 && display) {
    System.out.printf("%s\n",rc);
  }
  return rc;
}
