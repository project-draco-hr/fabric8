{
  long size=in.getFrameSize();
  if (size < MAX_SIZE && display) {
    System.out.printf("\n%s",in);
  }
  Buffer buf=write(in);
  if (size < MAX_SIZE && display) {
    System.out.printf("\n%s",string(buf.data));
  }
  AmqpFrame out=read(buf);
  if (size < MAX_SIZE && display) {
    System.out.printf("\n%s\n",out);
  }
  return out;
}
