{
switch (msg.getEventType()) {
case Notification.SET:
case Notification.ADD:
case Notification.ADD_MANY:
{
      if (destinationDataEventListener != null)       destinationDataEventListener.updated(destinationName);
      break;
    }
case Notification.REMOVE:
case Notification.REMOVE_MANY:
{
    if (destinationDataEventListener != null)     destinationDataEventListener.deleted(destinationName);
    break;
  }
}
super.notifyChanged(msg);
}
