{
  assertValid();
  try {
    return new String(ZkPath.loadURL(curator.get(),value),"UTF-8");
  }
 catch (  KeeperException.NoNodeException e) {
    LOGGER.warn("Could not load property value: {}. Ignoring.",value,e);
    return "";
  }
catch (  Exception e) {
    throw FabricException.launderThrowable(e);
  }
}
