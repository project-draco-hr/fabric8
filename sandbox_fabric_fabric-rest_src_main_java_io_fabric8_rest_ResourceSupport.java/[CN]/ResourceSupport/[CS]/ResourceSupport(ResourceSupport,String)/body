{
  this.parent=parent;
  this.messageContext=parent.messageContext;
  this.fabricService=parent.getFabricService();
  this.curator=parent.getCurator();
  this.baseUri=parent.getBaseUri();
  if (baseUri == null) {
    baseUri="";
  }
  if (Strings.notEmpty(pathPrefix)) {
    this.baseUri+=pathPrefix;
  }
}
