{
  if (manifest == null) {
    try {
      File jarFile=connection.getJarFile();
      if (jarFile != null && jarFile.exists()) {
        manifest=ContainerManifests.getContainerManifest(jarFile);
      }
    }
 catch (    IOException e) {
      manifest=new ContainerManifest();
    }
  }
  return manifest;
}
