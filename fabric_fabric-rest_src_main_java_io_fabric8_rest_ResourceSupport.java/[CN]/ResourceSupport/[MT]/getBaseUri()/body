{
  if (baseUri == null) {
    if (messageContext != null) {
      UriInfo uriInfo=messageContext.getUriInfo();
      if (uriInfo != null) {
        URI baseUriObject=uriInfo.getBaseUri();
        if (baseUriObject != null) {
          this.baseUri=baseUriObject.toString();
        }
      }
    }
    if (baseUri == null) {
      baseUri="";
    }
  }
  return baseUri;
}
