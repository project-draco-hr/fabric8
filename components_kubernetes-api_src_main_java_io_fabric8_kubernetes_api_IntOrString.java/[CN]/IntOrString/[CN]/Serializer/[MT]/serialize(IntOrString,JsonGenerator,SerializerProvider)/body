{
  if (value != null) {
    Integer intValue=value.getIntValue();
    if (intValue != null) {
      jgen.writeNumber(intValue);
    }
 else {
      String stringValue=value.getStringValue();
      if (stringValue != null) {
        jgen.writeString(stringValue);
      }
 else {
        jgen.writeNull();
      }
    }
  }
 else {
    jgen.writeNull();
  }
}
