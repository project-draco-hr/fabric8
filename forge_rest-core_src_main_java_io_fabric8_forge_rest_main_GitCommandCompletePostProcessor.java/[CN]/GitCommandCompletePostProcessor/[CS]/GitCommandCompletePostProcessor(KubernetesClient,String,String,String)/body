{
  this.kubernetes=kubernetes;
  this.gogsUrl=gogsUrl;
  this.gitUser=gitUser;
  this.gitPassword=gitPassword;
  this.address=gogsUrl.toString();
  int idx=address.indexOf("://");
  if (idx > 0) {
    address="http" + address.substring(idx);
  }
  if (!address.endsWith("/")) {
    address+="/";
  }
}
