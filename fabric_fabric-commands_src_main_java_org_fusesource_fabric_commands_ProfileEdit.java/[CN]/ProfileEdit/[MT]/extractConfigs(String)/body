{
  Map<String,String> configMap=new HashMap<String,String>();
  String key=null;
  String value=null;
  if (configs.contains("=")) {
    key=configs.substring(0,configs.indexOf("="));
    value=configs.substring(configs.indexOf("=") + 1);
  }
 else {
    key=configs;
    value=null;
  }
  if (key != null && !key.isEmpty()) {
    configMap.put(key,value);
  }
  return configMap;
}
