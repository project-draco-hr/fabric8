{
  if (entity != null) {
    return Strings.firstNonBlank(entity.getName(),getAdditionalPropertyText(entity.getAdditionalProperties(),"name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","id"),getAdditionalPropertyText(entity.getAdditionalProperties(),"id"),entity.getUid());
  }
 else {
    return null;
  }
}
