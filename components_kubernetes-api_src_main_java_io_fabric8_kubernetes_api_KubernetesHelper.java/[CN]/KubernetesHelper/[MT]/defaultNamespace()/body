{
  String namespace=Systems.getSystemPropertyOrEnvVar(KUBERNETES_NAMESPACE_SYSTEM_PROPERTY,KUBERNETES_NAMESPACE_ENV,null);
  if (Strings.isNullOrBlank(namespace)) {
    namespace=findDefaultKubernetesNamespace();
  }
  if (Strings.isNotBlank(namespace)) {
    return namespace;
  }
  return DEFAULT_NAMESPACE;
}
