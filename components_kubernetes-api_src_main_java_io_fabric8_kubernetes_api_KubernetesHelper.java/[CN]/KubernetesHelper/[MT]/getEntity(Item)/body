{
  if (item != null) {
    String kind=item.getKind();
    if (kind != null) {
      String json=toJson(item);
      byte[] bytes=json.getBytes();
      Object entity=loadEntity(bytes,kind,null);
      if (entity instanceof Entity) {
        return (Entity)entity;
      }
    }
  }
  return null;
}
