{
  if (entity != null) {
    return Strings.firstNonBlank(getName(entity.getMetadata()),getAdditionalPropertyText(entity.getAdditionalProperties(),"name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","id"));
  }
 else {
    return null;
  }
}
