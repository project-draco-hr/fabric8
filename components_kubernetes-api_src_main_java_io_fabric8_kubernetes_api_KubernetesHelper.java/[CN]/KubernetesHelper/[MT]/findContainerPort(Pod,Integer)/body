{
  List<Container> containers=KubernetesHelper.getContainers(pod);
  for (  Container container : containers) {
    List<Port> ports=container.getPorts();
    for (    Port port : ports) {
      if (Objects.equal(portNumber,port.getContainerPort())) {
        return port;
      }
    }
  }
  return null;
}
