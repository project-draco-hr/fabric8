{
  if (entity != null) {
    return Strings.firstNonBlank(entity.getMetadata().getName(),getAdditionalPropertyText(entity.getAdditionalProperties(),"name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","name"),getAdditionalNestedPropertyText(entity.getAdditionalProperties(),"metadata","id"),entity.getMetadata().getUid());
  }
 else {
    return null;
  }
}
