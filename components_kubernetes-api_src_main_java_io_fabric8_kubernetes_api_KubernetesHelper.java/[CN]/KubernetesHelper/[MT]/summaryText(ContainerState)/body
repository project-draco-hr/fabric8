{
  ContainerStateRunning running=entity.getRunning();
  if (running != null) {
    return "Running";
  }
  ContainerStateWaiting waiting=entity.getWaiting();
  if (waiting != null) {
    return "Waiting";
  }
  ContainerStateTerminated termination=entity.getTermination();
  if (termination != null) {
    return "Terminated";
  }
  return "Unknown";
}
