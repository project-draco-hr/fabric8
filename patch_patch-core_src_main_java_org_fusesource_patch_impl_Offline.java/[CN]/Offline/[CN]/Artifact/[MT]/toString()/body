{
  StringBuilder sb=new StringBuilder();
  sb.append(groupId).append(":").append(artifactId).append(":").append(version);
  if (!"jar".equals(type) || classifier != null) {
    sb.append(":").append(type);
    if (classifier != null) {
      sb.append(":").append(classifier);
    }
  }
  return sb.toString();
}
