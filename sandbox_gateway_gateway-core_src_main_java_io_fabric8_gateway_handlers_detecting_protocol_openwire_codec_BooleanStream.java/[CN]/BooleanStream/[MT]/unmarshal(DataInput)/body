{
  arrayLimit=(short)(dataIn.readByte() & 0xFF);
  if (arrayLimit == 0xC0) {
    arrayLimit=(short)(dataIn.readByte() & 0xFF);
  }
 else   if (arrayLimit == 0x80) {
    arrayLimit=dataIn.readShort();
  }
  if (data.length < arrayLimit) {
    data=new byte[arrayLimit];
  }
  dataIn.readFully(data,0,arrayLimit);
  clear();
}
