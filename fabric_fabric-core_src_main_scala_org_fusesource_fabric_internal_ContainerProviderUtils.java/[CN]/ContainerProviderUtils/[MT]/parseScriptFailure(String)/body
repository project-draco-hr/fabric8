{
  String error="";
  if (output != null) {
    String[] lines=output.split("\n");
    for (    String line : lines) {
      if (line.contains(FAILURE_PREFIX)) {
        try {
          error=line.substring(line.lastIndexOf(FAILURE_PREFIX) + FAILURE_PREFIX.length());
        }
 catch (        Throwable t) {
          error="Unknown error";
        }
        return error;
      }
    }
  }
  return error;
}
