{
  String error="";
  if (output != null) {
    String[] lines=output.split("\n");
    for (    String line : lines) {
      if (line.contains(FAILURE_PREFIX)) {
        error=error.substring(line.lastIndexOf(FAILURE_PREFIX) + FAILURE_PREFIX.length());
        return error;
      }
    }
  }
  return error;
}
