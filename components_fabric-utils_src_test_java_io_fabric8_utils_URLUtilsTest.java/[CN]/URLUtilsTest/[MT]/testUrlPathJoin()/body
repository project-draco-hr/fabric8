{
  assertEquals("http://foo.com/bar",URLUtils.urlPathJoin("http://foo.com/","bar"));
  assertEquals("http://foo.com/bar",URLUtils.urlPathJoin("http://foo.com","bar"));
  assertEquals("http://foo.com/bar",URLUtils.urlPathJoin("http://foo.com","/bar"));
  assertEquals("http://foo.com/bar",URLUtils.urlPathJoin("http://foo.com/","/bar"));
}
