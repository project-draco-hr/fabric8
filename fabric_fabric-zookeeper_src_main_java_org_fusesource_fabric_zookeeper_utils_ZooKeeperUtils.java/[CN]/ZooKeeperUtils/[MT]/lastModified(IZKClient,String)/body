{
  long lastModified=0;
  List<String> children=zooKeeper.getChildren(path);
  if (children.isEmpty()) {
    return zooKeeper.exists(path).getMtime();
  }
 else {
    for (    String child : children) {
      lastModified=Math.max(lastModified(zooKeeper,path + "/" + child),lastModified);
    }
  }
  return lastModified;
}
