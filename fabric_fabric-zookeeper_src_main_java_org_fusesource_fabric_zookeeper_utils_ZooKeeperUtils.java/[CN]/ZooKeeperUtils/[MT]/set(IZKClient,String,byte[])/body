{
  if (ZookeeperCommandBuilder.exists(path).execute(zooKeeper) != null) {
    ZookeeperCommandBuilder.set(path,value).execute(zooKeeper);
  }
  try {
    ZookeeperCommandBuilder.set(path,value).execute(zooKeeper);
  }
 catch (  KeeperException.NodeExistsException e) {
    ZookeeperCommandBuilder.set(path,value).execute(zooKeeper);
  }
}
