{
  if (pathSegment == null) {
    return false;
  }
  if (pathSegment.startsWith("{") && pathSegment.endsWith("}")) {
    String parameterName=pathSegment.substring(1,pathSegment.length() - 1);
    parameterNameValues.put(parameterName,actualSegment);
    return true;
  }
 else {
    return actualSegment != null;
  }
}
