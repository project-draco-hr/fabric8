{
  if (type.getProvides() != null) {
    cls()._implements(cm.ref(generator.getInterfaces() + "." + toJavaClassName(type.getProvides())));
  }
 else {
    cls()._implements(cm.ref(generator.getAmqpBaseType()));
  }
  createInitialFields();
  createStaticBlock();
  createGetArrayConstructor();
  generator.registry().cls().init().add(JExpr._new(cls()));
  sizeOfConstructor();
  sizeOfBody();
  size();
  write();
  writeConstructor();
  writeBody();
  read();
}
